    <script>
        //初始化加载
        $(function () {
            GetSafeDays();
            window.setInterval("refreshData()", 50000);
            //清除监控标识localStorage
            window.localStorage.removeItem("KMSBJcameraIndexCode");
        });


        //定时刷新数据
        function refreshData() {
            HighRiskWorking();
            NoCloseLoopHidStatistics();
            HiddenChangeForLeaderCockpit();
            LllegalChangeForLeaderCockpit();
            NoCloseLoopLllegalStatistics();
            GetKmIndexConfigure();
            refreshPerson();
        }

        //首页配置信息
        function GetKmIndexConfigure() {
            $.post("../login/GetKmIndexConfigure", {}, function (data) {
                GetSafeDays();
            });
        };

        //安全生产天数
        function GetSafeDays() {
            var Days = "@ViewBag.SafeDay";//安全生产天数
            if (Days.length > 0) {
                switch (Days.length) {
                case 1:
                    $(".SafeDay1").html(Days);
                    break;
                case 2:
                    $(".SafeDay1").html(Days.substr(1, 1));
                    $(".SafeDay2").html(Days.substr(0, 1));
                    break;
                case 3:
                    $(".SafeDay1").html(Days.substr(2, 1));
                    $(".SafeDay2").html(Days.substr(1, 1));
                    $(".SafeDay3").html(Days.substr(0, 1));
                    break;
                case 4:
                    $(".SafeDay1").html(Days.substr(3, 1));
                    $(".SafeDay2").html(Days.substr(2, 1));
                    $(".SafeDay3").html(Days.substr(1, 1));
                    $(".SafeDay4").html(Days.substr(0, 1));
                    break;
                default:
                }
            }
        }


        var KMHikImgIp = "@ViewBag.KMHikImgIp";//海康图片访问ip地址
    </script>

