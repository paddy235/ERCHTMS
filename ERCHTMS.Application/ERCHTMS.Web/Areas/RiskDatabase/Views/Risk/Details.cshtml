                $("#FaultType").attr("isvalid", "yes");
                $("#WorkTask").removeAttr("isvalid"); $("#WorkTask").attr("checkexpession");
                
                $("#MajorName").removeAttr("isvalid", "yes");
                $("#Description").removeAttr("isvalid", "yes");
                $("#HarmType").removeAttr("isvalid", "yes");
                $("#HarmProperty").removeAttr("isvalid", "yes");
                $("#DangerSource").removeAttr("checkexpession");
