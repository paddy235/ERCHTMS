<div style="margin:10px;" id="container">

    <div class="panel panel-default">
        <div class="panel-heading">
            <strong>&nbsp;&nbsp;风险辨识</strong>
            <span class="tools pull-right">
                <a class="fa fa-chevron-down" title="展开/收起"></a>
            </span>
        </div>
        <div class="panel-body">
            <table class="form" id="tabContent">
                <tr id="tr">
                    <td class="formTitle" style="width:150px;">风险类别<font face="宋体">*</font></td>
                    <td class="formValue">
                        <select id="RiskType" class="form-control" isvalid="yes" checkexpession="NotNull" onchange="selRiskType(this)">
                            @Html.Raw(new ERCHTMS.Busines.SystemManage.DataItemDetailBLL().GetOptionsString("RiskType"))
                        </select>
                    </td>
                </tr>
                <tr id="tr0">
                    <td class="formTitle" style="width:150px;">所属区域<font face="宋体">*</font></td>
                    <td class="formValue">
                        <input type="hidden" id="Id" value="@Guid.NewGuid().ToString()" />
                        <select id="AreaId" isvalid="yes" checkexpession="NotNull" class="form-control">
                            <option value="">请选择</option>
                            @Html.Raw(new ERCHTMS.Busines.RiskDatabase.DangerSourceBLL().GetOptionsStringForArea("0"))
                        </select>
                    </td>
                </tr>
                <tr id="tr10" style="display:none;">
                    <td class="formTitle">风险点<font face="宋体">*</font></td>
