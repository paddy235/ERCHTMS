            case "作业":
                $("#tabContent").find("tr").show();
                $("#tr80").hide(); $("#tr5").show();
                $("#tr1").hide(); $("#tr2").hide(); $("#tr3").show(); $("#tr4").show(); $("#tr0").show();
                $("#tr10").hide(); $("#tr20").hide(); $("#tr30").hide(); $("#tr40").hide();
                $("#WorkTask").attr("isvalid", "yes"); $("#WorkTask").attr("checkexpession", "NotNull");
              
                $("#EquipmentName").removeAttr("isvalid"); $("#EquipmentName").attr("checkexpession");
                //$("#Parts").removeAttr("isvalid"); $("#Parts").attr("checkexpession");
                $("#MajorName").removeAttr("isvalid", "yes");
                $("#Description").removeAttr("isvalid", "yes");
                $("#HarmType").removeAttr("isvalid", "yes");
                $("#HarmProperty").removeAttr("isvalid", "yes");
                $("#FaultType").removeAttr("checkexpession");
                $("#FaultType").removeAttr("isvalid", "yes");
                $("#DangerSource").attr("checkexpession", "NotNull");
                $("#DangerSource").attr("isvalid", "yes");
                break;
            case "职业病危害":
                $("#tabContent").find("tr").hide(); $("#tr80").hide(); $("#tr5").hide();
                $("#tr").show(); $("#tr10").show(); $("#tr20").show(); $("#tr30").show(); $("#tr40").show(); $("#tr0").show();
                $("#tr50").show(); $("#tr60").show(); $("#tr70").show();
