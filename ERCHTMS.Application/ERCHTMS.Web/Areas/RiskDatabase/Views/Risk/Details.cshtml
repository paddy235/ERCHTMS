    ViewBag.Title = "区域管理";
    Layout = "~/Views/Shared/_Index.cshtml";
}
<link rel="stylesheet" href="~/content/scripts/plugins/bootstrap-select/bootstrap-select.min.css" type="text/css" />
<script type="text/javascript" src="~/content/scripts/plugins/bootstrap-select/bootstrap-select.min.js"></script>
<script src="~/Content/scripts/business/Common.js"></script>
<script>
    var keyValue = request('keyValue');
    var parentId = request('parentId');
    var mode = request('mode');
    var areaId = request('areaId');
    var areaName = request('areaName');
    var tCode = ""; var tName = ""; var tContent = "";
    $(function () {
        InitGkcj();
        initControl();
        GetGrid();
    })
    //初始化控件
    function initControl() {
        //绑定内置岗位
        $.ajax({
            url: "../../RiskDatabase/DangerSource/GetPostTreeJson",
            async: false,
            type: "post",
            success: function (data) {
                $("#PostName").html('<option value="">请选择</option>');
                $("#PostName").append(data);
            }
        });
        //获取表单
        if (!!keyValue) {
            $.SetForm({
                url: "../../RiskDatabase/Risk/GetFormJson",
                param: { keyValue: keyValue },
                success: function (data) {
                    $("#container").formDeserialize(data);
                    //if (data.AccidentType != null && data.AccidentType != undefined) {
                    //    $("#AccidentType").val(data.AccidentType);
                    //    $("#AccidentType").find("option").each(function (i, dom) {
                    //        if (data.AccidentType.indexOf(dom.value) >= 0 && dom.value.length > 0) {
                    //            $(dom).attr("selected", "selected");
                    //        }
                    //    });
                    //}
                    if (data.RiskType != null && data.RiskType != undefined) {
                        $("#RiskType").val(data.RiskType);
                        $("#RiskType").find("option").each(function (i, dom) {
                            if (data.RiskType.indexOf(dom.value) >= 0 && dom.value.length > 0) {
                                $(dom).attr("selected", "selected");
                            }
                        });
                    }
                    if (data.HarmType != null && data.HarmType != undefined) {
                        $("#HarmType").val(data.HarmType);
                        $("#HarmType").find("option").each(function (i, dom) {
                            if (data.HarmType.indexOf(dom.value) >= 0 && dom.value.length > 0) {
                                $(dom).attr("selected", "selected");
                            }
                        });
                    }
                    selRiskType(document.getElementById("RiskType"));
                    selWay(document.getElementById("Way"), 0);
                }
            });
            
        } else {
            $("#table10").find(".form-control").attr("isvalid", "yes");
            $("#table10").find(".form-control").attr("checkexpession", "NotNull");
                //$("#tab10").find(".form-control").parent().prev().append('<font face="宋体">*</font>');
        }
    }
    //保存表单
    function AcceptClick() {
        if (!$('#container').Validform()) {
            return false;
        }
        var json = "";
        var postData = $("#container").formSerialize(keyValue);
        var arr0 = new Array("", "1", "2", "3", "4");
        var arr = [];
        $(arr0).each(function (j, idx) {
            var measures = $("#gridTable" + idx).jqGrid('getRowData');
            if (measures.length > 0) {
                $(measures).each(function (i, dom) {
                    arr.push({ Content: $("#gridTable" + idx).find("textarea[name='Content']").eq(i).val(), AreaId: $("#AreaId").val(), RiskId: $("#Id").val(), AreaCode: $("#AreaCode").val(), TypeName: $("#gridTable" + idx).attr("typename") });
                });
                json = JSON.stringify(arr);
            }
        });

        //postData["AccidentName"] = $("button[data-id='AccidentType']").attr("title").replace(/\s+/, "");
        postData["AreaName"] = $("#AreaId").find("option:selected").text();
        postData["AreaCode"] = $("#AreaId").find("option:selected").attr("code");
        postData["measuresJson"] = json;
        $.SaveForm({
            url: "../../RiskDatabase/Risk/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function () {
                if (top.win != undefined) {
                    top.win.initControl();
                    top.win.$("#gridTable").jqGrid('setGridParam', {
                        postData: { areaId: areaId }
                    }).trigger('reloadGrid');

                } else {
                    if (typeof ($.currentIframe().window.initControl) != "undefined") {
                        $.currentIframe().window.initControl();
                    }
                }
                $.currentIframe().window.$("#gridTable").trigger('reloadGrid');
            }
        })
    }
    var selectedRowIndex = 0;
    //加载表格
    function GetGrid() {
        var arr = new Array("", "1", "2", "3", "4");
        var types = new Array("工程技术", "管理", "个体防护", "培训教育", "应急处置");
        $(arr).each(function (i, idx) {
            var $gridTable = $("#gridTable" + idx);
            $gridTable.jqGrid({
                url: "../../RiskDatabase/Measures/GetListJson",
                postData: { riskId: !!keyValue ? keyValue : "-1", typeName: types[i] },
                datatype: "json",
                height: 200,
                width: $(window).width() - 100,
                cellEdit: true,
                sortable: false,
                colModel: [
                    { label: '主键', name: 'id', hidden: true },
                    { label: 'Code', name: 'typename', hidden: true },
                    {
                        label: '操作', name: 'Oper', index: 'Oper', width: 60, align: 'center', sortable: false,
                        formatter: function (cellvalue, options, rowObject) {
                            var html = "";
                            // html += "<a href=javascript:edit('" + rowObject.TypeCode + "','" + encodeURIComponent(rowObject.TypeName) + "','" + encodeURIComponent(rowObject.Content) + "','"+rowObject.Id+"')  title='修改'><i class='fa fa-pencil-square-o'></i></a>";
                            html += "<a onclick=del('" + rowObject.id + "',this," + rowObject.status + ")  title='删除'><i class='fa fa-trash-o'></i></a>";
                            return html;
                        }
                    },
                    //{ label: '措施类型', name: 'TypeName', width: 300, align: 'center' },
                    {
                        label: '具体措施', name: 'content', width: 300, align: 'left', headerAlign: "center", sortable: false
                        , formatter: function (cellvalue, options, rowObject) {
                            var html = '<textarea type="text"  name="Content" role="textbox" style="width: 100%;height:50px;" class="form-control" >' + cellvalue + '</textarea>';
                            return html;
                        }
                    }
                ],
                // pager: "#gridPager",
                viewrecords: true,
                rowNum: 20,
                rowList: [20, 50, 100],
                rownumbers: true

            });
            //查询事件
            $("#btn_Search").click(function () {
                $gridTable.jqGrid('setGridParam', {
                    postData: { value: nodeId, keyword: $("#txt_Keyword").val() },
                }).trigger('reloadGrid');
            });
        });


    }
    //编辑
    function edit(typeCode, typeName, content, id) {
        tCode = typeCode; tName = typeName; tContent = content;
        var title = "采取的风险控制措施";
        idx1 = dialogOpen({
            id: "Measure",
            title: "采取的风险控制措施",
            url: '/RiskDatabase/Measures/Form?keyValue=' + id,
            width: "500px",
            height: "400px",
            callBack: function (iframeId) {
                top.document.getElementById(iframeId).contentWindow.AcceptClick();
            }
        });
    }
    //删除管控措施
    function del(id, obj, status) {
        var jqTable = $(obj).parent().parent().parent().parent();
        if (status == 1) {
            $.RemoveForm({
                url: "../../RiskDatabase/Measures/RemoveForm",
                param: { keyValue: id },
                success: function (data) {
                    jqTable.delRowData(jqTable.getGridParam('selrow'));
                }
            })
        } else {
            var idx = dialogConfirm("确认删除吗？", function (isSure) {
                if (isSure) {
                    jqTable.delRowData(jqTable.getGridParam('selrow'));
                    top.layer.close(idx);
                } else {
                    top.layer.close(idx);
                }
            });
        }
    }
    //管控措施
    var idx1 = null;
    function addMeasure(obj) {
        var $li = $(".nav-tabs").find("li[class='active']");
        var index = $li.data("index");
        var name = $li.text();
        tCode = ""; tName = ""; tContent = "";
        areaId = $("#Area").attr("data-value");
        var areaCode = $("#Area").attr("data-code");
        var rowId = $("#gridTable" + index).jqGrid('getRowData').length;
        $("#gridTable" + index).addRowData(rowId, { id: rowId + 1, content: "", areaId: areaId, riskId: $("#Id").val(), areaCode: areaCode, typename: name, status: 0 });
        $("#gview_gridTable" + index).find(".unwritten").hide();
    }
    var node = null; var score = 0; var idx = null;
    function setScore(obj, title) {
        node = obj;
        var clsName = $("#Way").val() + obj.id;
        var selId = $(node).parent().parent().parent().children().eq(0).find("select").attr("id");
        clsName = selId == "Way" ? clsName : clsName.substring(0, clsName.length - 1);
        var width = $("#Way").val() == "LEC" ? "600px" : $("#Way").val() == "TRA" ? "900px" : "700px";
        var height = $("#Way").val() == "TRA" ? "600px" : "400px";
        idx = dialogContent({
            id: "win1",
            title: $(obj).parent().prev().text(),
            btns: 1,
            btn: ["关闭"],
            content: $("." + clsName).html(),
            width: width,
            height: height,
            callBack: function (iframeId) {
                top.layer.close(idx);
            }
        });
    }
    var mark = false;
    function getScore(score) {
        $(node).val(score);
        var selId = $(node).parent().parent().parent().children().eq(0).find("select").attr("id");
        var v1 = $.trim($("#ItemA").val());
        var v2 = $.trim($("#ItemB").val());
        var v3 = $.trim($("#ItemC").val());
        var r = $("#ItemR");
        var g = $("#Grade");
        if ($("#Way").val() == "TRA") {
            if (v1.length > 0 && v2.length > 0) {
                var result = 0;
                result = v1 * v2;
                result = result.toFixed(2);
                r.val(result);
                if (result >= 20) {
                    g.val("重大风险"); g.next().val("1");
                }
                else if (result >= 10 && result < 20) {
                    g.val("较大风险"); g.next().val("2");
                }
                else if (result >= 5 && result < 10) {
                    g.val("一般风险"); g.next().val("3");
                } else {
                    g.val("低风险"); g.next().val("4");
                }
            }
        } else {
            if (v1.length > 0 && v2.length > 0 && v3.length > 0) {
                var result = 0;
                result = v1 * v2 * v3;
                result = result.toFixed(2);
                r.val(result);
                if (result >= 400) {
                    g.val("重大风险"); g.next().val("1");
                }
                else if (result >= 200 && result < 400) {
                    g.val("较大风险"); g.next().val("2");
                }
                else if (result >= 70 && result < 200) {
                    g.val("一般风险"); g.next().val("3");
                } else {
                    g.val("低风险"); g.next().val("4");
                }
            }
        }
        top.layer.close(top.win.idx);
    }
    function selWay(obj, type) {
        var val = $(obj).val();
        var arr = null;
        if (obj.id == "Way1") {
            mark = true;
        }
        if (val == "LEC") {
            arr = new Array("事故发生的可能性L<font face='宋体'>*</font>", "暴露于危险环境的频繁程度E<font face='宋体'>*</font>", "发生事故产生的后果C<font face='宋体'>*</font>");
        } else if (val == "PSE") {
            arr = new Array("可能性 P<font face='宋体'>*</font>", "后果 S<font face='宋体'>*</font>", "暴露 E<font face='宋体'>*</font>");
        } else {
            arr = new Array("事故发生的可能性L<font face='宋体'>*</font>", "事故后果严重程度S<font face='宋体'>*</font>");
        }
        for (var j = 1; j < 4; j++) {
            $("." + obj.id + j).html(arr[j - 1]);
            //if (val == "TRA" && j == 3) {
            //    $("." + obj.id + j).hide();
            //    $("." + obj.id + j).next().hide();
            //} else {
            //    $("." + obj.id + j).show();
            //    $("." + obj.id + j).next().show();
            //}
        }
        if (type == 1) {
            $("#tab10").find("input[class='form-control']").val('');
        }

        for (var j = 1; j < 4; j++) {
            if (val == "TRA" && j == 3) {
                $("." + obj.id + j).next().find("input").removeAttr("checkexpession");
                $("." + obj.id + j).next().find("input").removeAttr("isvalid", "yes");
                $("." + obj.id + j).hide();
                $("." + obj.id + j).next().hide();
            } else {
                $("." + obj.id + j).next().find("input").attr("checkexpession", "NotNull");
                $("." + obj.id + j).next().find("input").attr("isvalid", "yes");
                $("." + obj.id + j).show();
                $("." + obj.id + j).next().show();
            }
        }

    }
    //选择区域时获取危险点信息
    function selArea(obj, select, val) {
        $.ajax({
            url: "../../RiskDatabase/Risk/GetRisks",
            data: { parentId: $(obj).val() },
            type: "post",
            dataType: "html",
            async: false,
            success: function (data) {
                select.html('<option value="">请选择</option>');
                select.append(data);
                if (!!val) {
                    select.val(val);
                }
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {

            }
        });
    }
