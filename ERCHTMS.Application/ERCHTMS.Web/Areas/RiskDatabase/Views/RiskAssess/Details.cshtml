                            var html = "";
                            // html += "<a href=javascript:edit('" + rowObject.TypeCode + "','" + encodeURIComponent(rowObject.TypeName) + "','" + encodeURIComponent(rowObject.Content) + "','"+rowObject.Id+"')  title='修改'><i class='fa fa-pencil-square-o'></i></a>";
