    function setScore(obj) {
        node = obj;
        var clsName = $("#Way").val() + obj.id;
        var selId = $(node).parent().parent().parent().children().eq(0).find("select").attr("id");
        clsName = selId == "Way" ? clsName : clsName.substring(0, clsName.length - 1);
