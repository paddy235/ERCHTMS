    //加载表格1
    function GetGridTable1(colModel,json) {
        $("#gridTable1").jqGrid("clearGridData");
        var $gridTable = $("#gridTable1");
        $gridTable.jqGrid({
            datatype: "local",
            data: json.rows,
            mtype: "post",
            autoheight: true,
            autowidth: true,
            colModel: colModel,
            viewrecords: true,
            rowNum: 100,
            rownumbers: true
        });
        $gridTable.jqGrid("setGridParam", {
            datatype: "local",
            data: json.rows,
            rowNums: json.rows.length
        }).trigger("reloadGrid");
    }

    //加载表格2
    function GetGridTable2() {
        var queryJson = {
            year: $("#Year").val()
        };
        $("#gridTable2").jqGrid({
            url: "../../EquipmentManage/SpecialEquipment/GetEquipmentFailureGridForSJ",
            postData: { queryJson: JSON.stringify(queryJson) },
            datatype: "json",
            mtype: "post",
            autoheight: true,
            height: 300,
            width: $(window).width() - 100,
            colModel: [
                { label: '电厂名称', name: 'typename', align: 'center', sortable: false },
                {
                    label: '数量', name: 'num', align: 'center', sortable: false,
                    formatter: function (cellvalue, options, rowObject) {
                        var html = "";
                        if (cellvalue == "0") {
                            return cellvalue;
                        }
                        else {
                            html += "<a href=javascript:openEquipmentFailure('" + rowObject.orgcode + "')>" + cellvalue + "</a>";
                        }
                        return html;
                    }
                }

            ],
            rowNum: 100,
            rownumbers: true
            , gridComplete: function () {

            }
        });
    }

