@{
    ViewBag.Title = "盲板信息";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script src="~/Content/scripts/business/Common.js" type="text/javascript"></script>
<script>
    var material = request("material");
    var specification = request("specification");
    var serialCode = request("serialcode");
    $(function () {
        $("#Material").val(material);
        $("#Specification").val(specification);
        $("#SerialCode").val(serialCode);
    })


    function AcceptClick(callback, winObject) {
        if (!$('#form1').Validform()) {
            return false;
        }

        if (callback) {
            material = $("#Material").val();
            specification = $("#Specification").val();
            serialCode = $("#SerialCode").val();

            callback({ material: material, specification: specification, serialCode: serialCode });
        }
        dialogClose();
    }
</script>

<div style="margin-top:20px;margin-right:30px;">
    <table class="form">
        <tr>
            <td class="formTitle" style="width:54px;">材质<font face="宋体">*</font></td>
            <td class="formValue">
                <input id="Material" type="text" class="form-control" placeholder="手动输入" isvalid="yes" checkexpession="NotNull" maxlength="20" />
            </td>
            <td class="formTitle" style="width:54px;">规格<font face="宋体">*</font></td>
            <td class="formValue">
                <input id="Specification" type="text" class="form-control" placeholder="手动输入" isvalid="yes" checkexpession="NotNull" maxlength="20" />
            </td>
            <td class="formTitle" style="width:54px;">编号<font face="宋体">*</font></td>
            <td class="formValue">
                <input id="SerialCode" type="text" class="form-control" placeholder="手动输入" isvalid="yes" checkexpession="NotNull" maxlength="20" />
            </td>
        </tr>
    </table>
</div>
