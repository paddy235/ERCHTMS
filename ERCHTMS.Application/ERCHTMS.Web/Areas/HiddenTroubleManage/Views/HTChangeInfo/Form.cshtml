    function CommonMethod() {
        //登记
        $("#btn_Save").click(function () {
            AcceptClick();
        });
        //提交流程
        $("#btn_Submit").click(function () {
            SubmitAction();
        });
        //转交
        $("#btn_Deliver").click(function () {
            var idx = dialogOpen({
                id: "DeliverForm",
                title: "隐患整改转交",
                url: '/HiddenTroubleManage/HTChangeInfo/DeliverForm?keyValue=' + keyValue + "&changeid=" + $("#CHANGEID").val(),
                width: ($(top.window).width() / 2) + "px",
                height: ($(top.window).height() / 2) + "px",
                btn: null,
                callBack: function (iframeId) {
                    top.layer.close(idx);
                }
            });
        });
        //整改结果
        $("input[name='CHANGERESULT']").click(function () {
            var isChangeValue = $(this).val();
            //已经完成
            if (isChangeValue == "1") {
                //是否回退
                $("input[name='ISBACK']").attr("disabled", "disabled");
                $("input[name='ISBACK']:eq(1)").prop("checked", "checked");
                //整改完成时间
                $("#CHANGEFINISHDATE").attr("isvalid", "yes");
                //实际治理资金
                $("#REALITYMANAGECAPITAL").attr("isvalid", "yes");
                $(".form:eq(0) tr:eq(2) td:eq(2)").html("整改完成时间<span style='color:red;'>*</span>"); //整改完成时间
                $(".form:eq(0) tr:eq(7) td:eq(2)").html("实际治理资金(元)<span style='color:red;'>*</span>"); //实际治理资金(元)

                $("#CHANGERESUME").attr("isvalid", "yes");
                $(".form:eq(0) tr:eq(4) td:eq(0)").html("整改情况描述<span style='color:red;'>*</span>");//整改情况描述
            }
            else {
                if (isenableback) {
                    //是否回退
                    $("input[name='ISBACK']").removeAttr("disabled");
                }
                //整改完成时间
                $("#CHANGEFINISHDATE").removeAttr("isvalid");
                //实际治理资金
                $("#REALITYMANAGECAPITAL").removeAttr("isvalid");
                $(".form:eq(0) tr:eq(2) td:eq(2) span").remove(); //整改完成时间
                $(".form:eq(0) tr:eq(7) span").remove(); //实际治理资金

                //整改情况描述
                $("#CHANGERESUME").removeAttr("isvalid");
                $(".form:eq(0) tr:eq(4) td:eq(0) span").remove(); //整改情况描述
            }

            //禁用是否回退
            if (isstopback) {
                $("input[name='ISBACK']:eq(1)").prop("checked", "checked");
                $("input[name='ISBACK']").each(function (index, ele) {
                    $(this).attr("disabled", "disabled");
                });
            }
        });
        //是否回退
        $("input[name='ISBACK']").click(function () {
            var isbackValue = $(this).val();
            if (isbackValue == "1") {
                $("#btn_Save").attr("disabled", "disabled");
                $("input[name='CHANGERESULT']").attr("disabled", "disabled");
                $("input[name='CHANGERESULT']:eq(1)").prop("checked", "checked");
                $(".form:eq(0) tr:eq(6)").css("display", "");
                //整改完成时间
                $("#CHANGEFINISHDATE").removeAttr("isvalid");
                //实际治理资金
                $("#REALITYMANAGECAPITAL").removeAttr("isvalid");
                //回退原因
                $("#BACKREASON").attr("isvalid", "yes");
                $(".form:eq(0) tr:eq(2) td:eq(2) span").remove(); //整改完成时间
                $(".form:eq(0) tr:eq(7) span").remove(); //实际治理资金
                $(".form:eq(0) tr:eq(6) td:eq(0)").html("回退原因<span style='color:red;'>*</span>"); //回退原因(元)

                //整改情况描述
                $("#CHANGERESUME").removeAttr("isvalid");
                $(".form:eq(0) tr:eq(4) td:eq(0) span").remove(); //整改情况描述
            }
            else {
                $("#btn_Save").removeAttr("disabled");
                $("input[name='CHANGERESULT']").removeAttr("disabled");
                $(".form:eq(0) tr:eq(6)").css("display", "none");
                //回退原因
                $("#BACKREASON").removeAttr("isvalid");
                $("#BACKREASON").val("");
            }
        });
    }

    //保存表单;
    function AcceptClick() {
        var postData = $("#form1").formSerialize(keyValue);
        var changeResultVal = $("input[name='CHANGERESULT']:checked").val();
        postData["CHANGERESULT"] = changeResultVal;
        var isBackVal = $("input[name='ISBACK']:checked").val();
        postData["ISBACK"] = isBackVal;
        $.SaveForm({
            url: "../../HiddenTroubleManage/HTChangeInfo/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function () {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
            }
        });
    }
    //提交流程
    function SubmitAction() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var changeResultVal = $("input[name='CHANGERESULT']:checked").val();
        var isBackVal = $("input[name='ISBACK']:checked").val();
        var subCount = 0;
        if (!!ControlPicMustUpload) {
            var array = new Array();
            if (ControlPicMustUpload.indexOf("HIDCHANGEPHOTO") >= 0) {
                array.push({ key: "HIDCHANGEPHOTO", value: $("#HIDCHANGEPHOTO").val() });
            }
            if (array.length > 0) {
                //绑定附件
                $.ajax({
                    url: '../../PublicInfoManage/ResourceFile/GetFilesByJsonData',
                    data: { jsonData: JSON.stringify(array) },
                    type: "post",
                    success: function (data) {
                        var yesSubmit = "";
                        if (!!data) {
                            var result = eval("(" + data + ")");
                            $(result).each(function (i, ele) {
                                var key = ele.key;
                                switch (key) {
                                    case "HIDCHANGEPHOTO":
                                        yesSubmit += parseInt(ele.value) > 0 ? "2" : ""; //隐患整改图片
                                        break;
                                }
                            });

                            var errorMsg = "请上传";
                            if (ControlPicMustUpload.indexOf("HIDCHANGEPHOTO") >= 0) {
                                if (yesSubmit.indexOf("2") >= 0) {
                                    subCount += 1;
                                }
                                else {
                                    errorMsg += "隐患整改图片,";
                                }
                            }
                            if (changeResultVal == "1" || isBackVal == "1") {
                                if (subCount == result.length) {
                                    SubmitData();
                                }
                                else {
                                    if (subCount < result.length) {
                                        errorMsg = errorMsg.substring(0, errorMsg.length - 1);
                                    }
                                    dialogMsg(errorMsg, 0);
                                }
                            }
                            else {
                                dialogMsg('请确认整改是否完成!', 0);
                            }
                        }
                    }
                });
            }
        }
        else {
            SubmitData();
        }
    }

    function SubmitData()
    {
        var postData = $("#form1").formSerialize(keyValue);
        var changeResultVal = $("input[name='CHANGERESULT']:checked").val();
        postData["CHANGERESULT"] = changeResultVal;
        var isBackVal = $("input[name='ISBACK']:checked").val();
        postData["ISBACK"] = isBackVal;

        //完成状态提交或者退回
        if (changeResultVal == "1" || isBackVal == "1") {
            $("#btn_Submit").attr("disabled", "disabled");
            $.SaveForm({
                url: "../../HiddenTroubleManage/HTChangeInfo/SubmitForm?keyValue=" + keyValue,
                param: postData,
                loading: "正在保存数据...",
                success: function () {
                    $.currentIframe().$("#gridTable").trigger("reloadGrid");
                    top.refreshWork();
                }
            })
            $("#btn_Submit").removeAttr("disabled");
        }
        else {
            dialogMsg('请确认整改是否完成!', 0);
        }
    }

    //上传文件
    function uploadify(objId, isimage) {
        var folderId = "";
        var fileType = 'bmp,ico,gif,jpeg,jpg,png,psd';
        var isdelete = actiontype == "view" ? false : true;
        switch (objId) {
            case "uploader_hidphoto": //隐患图片
                folderId = $("#HIDPHOTO").val();
                if (!folderId) {
                    folderId = "@Guid.NewGuid().ToString()";
                    $("#HIDPHOTO").val(folderId);
                }
                isdelete = false;
                break;
            case "uploader_hidchangephoto": //整改图片
                folderId = $("#HIDCHANGEPHOTO").val();
                if (!folderId) {
                    folderId = "@Guid.NewGuid().ToString()";
                    $("#HIDCHANGEPHOTO").val(folderId);
                }
                if (actiontype != "view") {
                    file_upload.init({
                        keyValue: folderId, extensions: fileType, isImage: isimage, el: '#uploader_hidchangephoto'
                    });
                }
                break;
            case "uploader_attachment":  //整改附件
                folderId = $("#ATTACHMENT").val();
                if (!folderId) {
                    folderId = "@Guid.NewGuid().ToString()";
                    $("#ATTACHMENT").val(folderId);
                }
                fileType = 'bmp,ico,gif,jpeg,jpg,png,psd,zip,rar,doc,docx,xls,xlsx,pdf,txt,chm';
                if (actiontype != "view") {
                    file_upload.init({
                        keyValue: folderId, extensions: fileType, isImage: isimage, el: '#uploader_attachment'
                    });
                }
                break;
        }

        $.ajax({
            url: '../../PublicInfoManage/ResourceFile/GetFilesByRecId',
            data: { recId: folderId },
            type: "post",
            success: function (data) {
                var objdata = eval("(" + data + ")"); //转化为对象类型
                file_upload.bind(objdata, isdelete, isimage, folderId, objId);
            }
        });
    }
    //隐患信息报告单
    function exportImportant() {  //隐患基本信息
        window.location.href = "../../HiddenTroubleManage/HTBaseInfo/ExportImportant?keyValue=" + keyValue;
    }
    //隐患排查档案
    function exportRecordInfo() {
        //HRDL专用
        var result = "@(new ERCHTMS.Busines.SystemManage.DataItemDetailBLL().GetItemValue("IsOpenPassword"))";
        if (result == "true") {
            Loading(true, "正在导出...");
            $.ajax({
                url: "../../SaftyCheck/SaftyCheckDataRecord/ImportResult",
                data: { checkId: keyValue, checkType: 2 },
                type: "post",
                dataType: "JSON",
                success: function (data) {
                    if (data.type == 1) {
                        window.location.href = "../../Utility/DownloadFile?filePath=~/Resource/temp/" + data.resultdata + "&newFileName=隐患排查档案.doc";
                    } else {
                        dialogAlert(data.message);
                    }
                    Loading(false);
                }
            });
        } else { //通用版本
            window.location.href = "../../HiddenTroubleManage/HTBaseInfo/ExportRecordInfo?keyValue=" + keyValue;
        }
    }

