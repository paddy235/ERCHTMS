            height: '680px',
            btn: null
        });
    }
    //整改效果评估列表
    function GetEstimateGrid() {
        var selectedRowIndex = 0;
        var $gridTable = $('#estimateGridTable');
        $gridTable.jqGrid({
            url: "../../HiddenTroubleManage/HTEstimate/GetHistoryListJson?keyCode=" + $("#HIDCODE").val(),
            datatype: "json",
            height: $(window).height() - 300,
            autowidth: true,
            colModel: [
                {
                    label: '操作', name: 'ID', index: 'ID', width: 120, align: 'center',
