    $(function () {
        $.ajax({
            type: "get",
            url: "../../HiddenTroubleManage/HTBaseInfo/IsEnableMinimalistMode",
            success: function (data) {
                //启用极简模式
                if (!!parseInt(data)) {
                    $(".tab0").remove();
                    $(".tab1").css("display", "");
                    MinimalistInit(); //极简版本
                    $(".hid").css("display", "none");
                }
                else { //专业版本
                    $(".tab1").remove();
                    $(".tab0").css("display", "");
                    initControl(); //电力版本
                }
            }
        });
    });

    //获取历史整改信息
    function GetHistoryChange() {
        var keyCode = $("#HIDCODE").val();
        dialogOpen({
            id: 'HistoryForm',
