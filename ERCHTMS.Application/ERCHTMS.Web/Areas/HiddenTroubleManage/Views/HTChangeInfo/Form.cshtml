            width: ($(top.window).width() - 200) + "px",
            height: '680px',
            btn: null
        });
    }
    //初始化控件
    function initControl() {
        //验证当前是否存在flash插件
        var fls = flashChecker();
        var s = "";
        if (!fls.f) {
            var html = "<a href='http://get.adobe.com/cn/flashplayer/' style='text-decoration:none;color:red;' target='_bank'>您的浏览器未安装Flash插件,无法支持文件上传功能,请下载安装!</a>";
            $("#HidMessage").html(html);
        }
        $(".center-Panel").css({ "width": "100%", "height": ($(top.window).height() - 160) + "px", "padding-top": "38px" });
        $.SetForm({
            url: "../../HiddenTroubleManage/HTBaseInfo/GetInitDataJson?keyValue=" + keyValue,
            success: function (data) {
                var IsDisable_MajorClassify = data.IsDisable_MajorClassify;//是否禁用专业分类
                ControlPicMustUpload = data.ControlPicMustUpload; //相关图片必传控制
                if (IsDisable_MajorClassify > 0) {
                    $(".majorTr td:eq(0)").css("display", "none");
                    $(".majorTr td:eq(1)").css("display", "none");
                    $("#MAJORCLASSIFY").removeAttr("isvalid");
                    data.MajorClassify = [];
                }
                //用于判定是否具有转交功能
                IsDeliver = data.IsDeliver;
                //为空则隐藏
                if (!IsDeliver) { $("#btn_Deliver").css("display", "none"); }
                //隐患整改图片
                $("#HIDCHANGEPHOTO").val(data.HidChangePhoto);
                if (!!data.ATTACHMENT) {
                    //隐患附件
                    $("#ATTACHMENT").val(data.ATTACHMENT);
                }
                else {
                    var attachmentID = "@Guid.NewGuid().ToString()";
                    $("#ATTACHMENT").val(attachmentID);
                }
                //检查类型
                $("#CHECKTYPE").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "==请选择==",
                    allowSearch: false,
                    data: data.CheckType
                });
                //隐患级别
                $("#HIDRANK").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "==请选择==",
                    allowSearch: false,
                    data: data.HidRank,
                });
                //隐患专业分类
                $("#MAJORCLASSIFY").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "==请选择==",
                    allowSearch: false,
                    data: data.MajorClassify,
                });
                //隐患类别
                $("#HIDTYPE").ComboBoxTree({
                    url: "../../HiddenTroubleManage/HTBaseInfo/GetHiddenTypeDataJson",
                    description: "==请选择==",
                    allowSearch: false
                });
                //所属单位
                $("#HIDDEPARTNAME").ComboBox({
                    id: "DepartmentId",
                    text: "FullName",
                    description: "===请选择===",
                    allowSearch: false,
                    data: data.DeptData
                });
            }
        });

        //整改完成情况
        $("input[name='CHANGERESULT']:eq(0)").prop("checked", "checked");
        //隐藏重大隐患填项
        $(".form:eq(2) .importHid").css("display", "none");
        //回退原因
        $(".form:eq(0) tr:eq(6)").css("display", "none");
        //是否回退
       // var isBackVal = $("input[name='ISBACK']:checked").val();
        //不回退
        //if (isBackVal == "0") {
        //}
        //获取表单
        if (!!keyValue) {
            $.SetForm({
                url: "../../HiddenTroubleManage/HTBaseInfo/GetFormJson",
                param: { keyValue: keyValue },
                success: function (data) {

                    //隐患整改
                    if (null != data.changeInfo) {
                        if (!data.changeInfo.HIDCHANGEPHOTO) {
                            data.changeInfo.HIDCHANGEPHOTO = $("#HIDCHANGEPHOTO").val();
                        }
                        $("#CHANGEID").val(data.changeInfo.ID);
                        $("#form1").formDeserialize(data.changeInfo);
                        //是否整改完成
                        if (!!data.changeInfo.CHANGERESULT)
                        {
                            if (data.changeInfo.CHANGERESULT == "1") {
                                $("input[name='CHANGERESULT']:eq(0)").prop("checked", "checked");
                            }
                            else {
                                $("input[name='CHANGERESULT']:eq(1)").prop("checked", "checked");
                            }
                        }
          
                        //是否回退
                        if (data.changeInfo.ISBACK == "1") {
                            $("input[name='ISBACK']:eq(0)").prop("checked", "checked");
                        }
                        else {
                            $("input[name='ISBACK']:eq(1)").prop("checked", "checked");
                        }
                    }
                    //隐患验收
                    if (null != data.acceptInfo) {
                        $("#ACCEPTID").val(data.acceptInfo.ID);
                        $("#form1").formDeserialize(data.acceptInfo);
                    }
                    //则禁用是否退回
                    if (!!data.isreformback && data.isreformback == 1) {
                        isenableback = false; //
                        //是否退回
                        $("input[name='ISBACK']:eq(1)").prop("checked", "checked"); //否
                        $("input[name='ISBACK']").attr("disabled", "disabled");
                    }
                    //隐患基本信息
                    if (null != data.baseInfo) {
                        //加载
                        LoadTips("yh" + data.baseInfo.HIDDEPART);

                        $("#form1").formDeserialize(data.baseInfo);
                        $("#CHECKTYPE").ComboBoxSetValue(data.baseInfo.CHECKTYPE);
                        $("#HIDRANK").ComboBoxSetValue(data.baseInfo.HIDRANK);
                        $("#MAJORCLASSIFY").ComboBoxSetValue(data.baseInfo.MAJORCLASSIFY);  //专业分类/隐患分类
                        $("#HIDTYPE").ComboBoxTreeSetValue(data.baseInfo.HIDTYPE); //隐患类别
                        $("#HIDDEPARTNAME").ComboBoxSetValue(data.baseInfo.HIDDEPART); //所属单位
                        //国电新疆版本,禁用回退功能
                        if (data.baseInfo.ADDTYPE == "3") {
                            isstopback = true;
                            $("input[name='ISBACK']").each(function (index, ele) {
                                $(this).attr("disabled", "disabled");
                            });
                        }
                        //是否挂牌督办
                        if (data.baseInfo.ISGETAFTER == "1") {
                            $("input[name='ISGETAFTER']:eq(0)").prop("checked", "checked");
                        }
                        else {
                            $("input[name='ISGETAFTER']:eq(1)").prop("checked", "checked");
                        }

                        //是否曝光
                        if (data.baseInfo.EXPOSURESTATE == "1") {
                            $("input[name='EXPOSURESTATE']:eq(0)").prop("checked", "checked");
                        }
                        else {
                            $("input[name='EXPOSURESTATE']:eq(1)").prop("checked", "checked");
                        }

                        var applicationstatus = data.changeInfo.APPLICATIONSTATUS;
                        //正在延期申请中的，无法进行转交
                        if (applicationstatus == "1") {
                            $("#btn_Deliver").remove();
                        }
                    }
                    //隐藏重大隐患填项
                    var rankname = $("#HIDRANK").ComboBoxGetText();
                    if (!!rankname) {
                        if (rankname.indexOf("重大") >= 0) {
                            //隐藏重大隐患填项
                            $(".form:eq(2) .importHid").css("display", "");
                        }
                    }


                    //当前隐患类别为设备设施的时候
                    var curTextbox = $("#HIDTYPE").text();
                    if (curTextbox.indexOf("设备设施") >= 0) {
                        $(".form:eq(2) tr:eq(4)").css("display", "");
                    }
                    else {
                        $(".form:eq(2) tr:eq(4)").css("display", "none");
                    }
                }
            })
        }
        uploadify("uploader_attachment", false); //整改附件
        uploadify("uploader_hidchangephoto", true); //整改图片
        uploadify("uploader_hidphoto", true); //隐患图片
        GetApprovalGrid();  //评估列表
        //查看下
        if (actiontype == "view") {
            $("#btn_Save").attr("disabled", "disabled");
            $("#btn_Submit").attr("disabled", "disabled");
            $("#btn_Deliver").attr("disabled", "disabled");
            $("#form1 input,textarea , .ui-select, .ui-select-text , .ui-select-option-content").each(function (ele, index) {
                $(this).attr("disabled", "disabled");
            });
        }
        $(".form-control").focus(function () {
            $(".poptip").remove();
        });
        //根据当前隐患，控制整改效果评估是否显示
        var hidRankText = $("#HIDRANK").ComboBoxGetText();
        //当前查看的是重大隐患
        if (hidRankText.indexOf("重大") >= 0) {
            $("#btn_ExportImportant").css("display", "");
            $("#div_recheck").css("display", "none");
            $("#div_estimate").css("display", "");
            //整改效果评估信息
            GetEstimateGrid();
        }
        else {
            $("#div_recheck").css("display", "");
            $("#div_estimate").css("display", "none"); //整改效果评估面板整体隐藏，一般隐患没有整改效果评估
            //复查验证信息
            GetRecheckGrid();
        }
        //公共方法
        CommonMethod();
    }

