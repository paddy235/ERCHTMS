@{;
    ViewBag.Title = "表单页面";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script src="~/Content/scripts/business/Common.js" type="text/javascript"></script>
<!--webUploader文件上传组件-->
<link href="~/content/scripts/plugins/webuploader/webuploader.css" rel="stylesheet" />
<link href="~/content/scripts/plugins/webuploader/webuploader-demo.css" rel="stylesheet" />
<script type="text/javascript" src="~/content/scripts/plugins/webuploader/webuploader.min.js"></script>
<script type="text/javascript" src="~/content/scripts/business/fileupload.js"></script>
<script src="~/Content/scripts/business/detailAgency.js"></script>
<script>
    //页面请求参数
    var keyValue = request('keyValue');
    var actiontype = 'view';//request('actiontype');

    $(function () {
        keyValue = !!keyValue ? keyValue : newGuid();
        //
        //创建代理
        //
        var detailAgc = new detailAgency({
            keyValue: keyValue,
            actiontype: actiontype
        });
        //
        //初始化数据
        //
        detailAgc.initialPage([
            {
                url: '../../ComprehensiveManage/InfoSubmitDetails/GetFormJson',
                param: { keyValue:keyValue },        
                upfiles: [{ Id: 'uploader',recIdExp:"data.data.ID", extensions: 'txt,doc,docx,pdf,xls,xlsx,rar,zip,ppt,pptx', actiontype: actiontype, isImage: false, fileDir: 'InfoSubmit',width:'100%' }]
            }
        ]);
    })
</script>
<table class="form">
    <tr>
        <td class="formValue">
            <div id="uploader" class="uploader" style="border:1px solid #ccc; margin-top:10px; min-height:120px; margin-bottom:10px;">
                <div class="queueList">
                    <div id="file_place" class="placeholder">
                        <div class="filePicker" style="margin-left:25px; margin-top:10px;"></div>
                    </div>
                </div>
                <div class="statusBar" style="display:none;">
                    <div class="progress">
                        <span class="text">0%</span>
                        <span class="percentage"></span>
                    </div>
                    <div class="info"></div>
                </div>
            </div>
        </td>
    </tr>
</table>