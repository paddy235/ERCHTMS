                $(arr).each(function (j, code) {
                    code = code + ",";
                    if (codes.indexOf(code) < 0) {
                        codes += code;
                    }
                });
            }
        });
        if (codes.length > 0) {
            codes = codes.substring(0, codes.length - 1);
        }
        return codes;
    }
