@using ERCHTMS.Code
@{;
ViewBag.Title = "表单页面";
Layout = "~/Views/Shared/_Form.cshtml";
}


<script src="~/Content/scripts/business/Common.js" type="text/javascript"></script>
<link href="~/content/scripts/plugins/icheck/skins/square/_all.css" type="text/css" rel="stylesheet" />
<script src="~/content/scripts/plugins/icheck/js/icheck.min.js" type="text/javascript"></script>
<script src="~/content/scripts/plugins/icheck/js/custom.min.js" type="text/javascript"></script>

<script src="~/Content/scripts/plugins/jquery.md5.js"></script>
<script src="~/Content/scripts/plugins/uploadify/ajaxfileupload.js"></script>


<!--webUploader文件上传组件-->
<link href="~/content/scripts/plugins/webuploader/webuploader.css" rel="stylesheet" />
<link href="~/content/scripts/plugins/webuploader/webuploader-demo.css" rel="stylesheet" />
<script type="text/javascript" src="~/content/scripts/plugins/webuploader/webuploader.min.js"></script>
<script type="text/javascript" src="~/content/scripts/business/fileupload.js"></script>
<script src="~/Content/scripts/plugins/combo-select/jquery.combo.select.js"></script>
<link rel="stylesheet" href="~/Content/scripts/plugins/combo-select/combo.select.css">

<link rel="stylesheet" href="~/content/scripts/plugins/bootstrap-select/bootstrap-select.min.css" type="text/css" />
<script type="text/javascript" src="~/content/scripts/plugins/bootstrap-select/bootstrap-select.min.js"></script>
<script>
    var keyValue = request('keyValue');
    var action = request('action');
    var ID;
    var LID;
    var LaborList;
    var isNew = 0; //默认0为新增物资 1为库里已有物资
    var lastrow;
    var lastcell;
    var selData = "";
    var sizelist;
    var InStocklist;
    var isPermission = false;//是否拥有权限
    $(function () {
        $('select').comboSelect();
        Ini();
        GetSizeList();
        GetInStockList();
        initControl();
        if (!!keyValue) {
            GetFiles();
            isNew = 1;
            btnReadonly();

            $("#Name").attr("readonly", "readonly");
            //使用期限和型号课改
            $("#Model").removeAttr("readonly");
            $("#TimeNum").removeAttr("readonly");
            $("#TimeType").removeAttr("readonly");
            $("#Note").removeAttr("readonly");
        }
        GetGrid();
        if (action == "show") {
            $("#DeptId").attr("readonly", "readonly");
            $("#PostId").attr("readonly", "readonly");
        }
    });


    //获取尺码下拉信息
    function GetSizeList() {
        $.ajax({
            url: "../../SystemManage/DataItemDetail/GetDataItemListJson",
            data: { EnCode: "LaborSize" },
            type: "GET",
            async: false,
            dataType: "Json",
            success: function (result) {
                sizelist = result;
            }
        });
    }


    //获取物品库存信息
    function GetInStockList() {
        $.ajax({
            url: "../../SystemManage/DataItemDetail/GetDataItemListJson",
            data: { EnCode: "LaborName" },
            type: "GET",
            async: false,
            dataType: "Json",
            success: function (result) {
                InStocklist = result;
            }
        });
    }

    //设置控件是否可填
    function btnReadonly() {
        if (isNew == 0) {
            $("#Model").removeAttr("readonly");
            $("#Type").removeAttr("disabled");
            $("#Unit").removeAttr("readonly");
            $("#TimeNum").removeAttr("readonly");
            $("#TimeType").removeAttr("readonly");
            $("#Note").removeAttr("readonly");
        } else {
            $("#Model").attr("readonly", "readonly");
            $("#Type").attr("disabled", "true");
            $("#Unit").attr("readonly", "readonly");
            $("#TimeNum").attr("readonly", "readonly");
            $("#TimeType").attr("readonly", "readonly");
            $("#Note").attr("readonly", "readonly");
        }
    }

    //判断是否可以选择部门
    function DeptSelect() {
        $.ajax({
            url: '../../LaborProtectionManage/Laborinfo/GetIsDept',
            type: "GET",
            async: false,
            dataType: "text",
            success: function (result) {
                var ret = result.split(',');
                if (!!keyValue) {

                } else {
                    $("#DeptId").ComboBoxSetValue(ret[0]);
                }
                //if (ret[1] == "true") {
                //    isPermission = true;
                //    $("#DeptId").removeAttr("readonly");
                //} else {
                //    isPermission = false;
                //    $("#DeptId").attr("readonly", "readonly");

                //}
                if (!!keyValue) {

                } else {
                    iniPost();
                }

            }
        });
    }

    function iniPost() {
        var value = $("#DeptId").attr('data-value');
        //加载岗位
        $("#PostId").ComboBox({
            url: top.contentPath + "/BaseManage/Post/GetPostJson?deptid=" + value,
            id: "RoleId",
            text: "FullName",
            description: "==请选择==",
            allowSearch: true
        }).bind("change", function () {
            DorpDownSize();
        });
    }

    function DorpDownSize() {

        var type = $("#Type").val();
        if (type == "衣服" || type == "鞋子") {
            for (var i = 0; i < sizelist.length; i++) {
                if (type == sizelist[i].ItemName) {
                    selData = sizelist[i].ItemValue;

                }
            }
        } else {
            selData = "";

        }

        DropRefeshGrid();
    }

    function IsEdit() {
        var type = $("#Type").val();
        if (type == "衣服" || type == "鞋子") {

            $("#gridTable").setColProp("Size", { editable: true, editoptions: { value: selData } });

        }
        else {
            $("#gridTable").setColProp("Size", { editable: false });

        }
    }

    function DropRefeshGrid() {

        url = "../../LaborProtectionManage/Laborinfo/GetLaborUser";
        data = { deptId: $("#DeptId").attr('data-value'), postId: $("#PostId").attr('data-value'), type: $("#Type").val() }

        $('#gridTable').jqGrid('setGridParam', {
            url: url,
            postData: data
        }).trigger('reloadGrid');

    }

    function RefeshGrid() {
        if (!!keyValue) {
            url = "../../LaborProtectionManage/Laborequipmentinfo/GetListJson";
            data = { queryJson: keyValue }
        } else {
            url = "../../LaborProtectionManage/Laborinfo/GetLaborUser";
            data = { deptId: $("#DeptId").attr('data-value'), postId: $("#PostId").attr('data-value'), type: $("#Type").val() }
        }
        $('#gridTable').jqGrid('setGridParam', {
            url: url,
            postData: data
        }).trigger('reloadGrid');

    }

    //加载表格
    function GetGrid() {
        var selectedRowIndex = 0;
        var $gridTable = $('#gridTable');
        var url;
        var data;
        if (!!keyValue) {
            url = "../../LaborProtectionManage/Laborequipmentinfo/GetListJson";
            data = { queryJson: keyValue }
        } else {
            url = "../../LaborProtectionManage/Laborinfo/GetLaborUser";
            data = { deptId: $("#DeptId").attr('data-value'), postId: $("#PostId").attr('data-value'), type: $("#Type").val() }
        }
        $gridTable.jqGrid({
            autowidth: true,
            height: 300,
            url: url,
            postData: data,
            datatype: "json",
            colModel: [
               { label: '主键', name: 'ID', hidden: true },
                { label: '用户id', name: 'UserId', hidden: true },
                {
                    label: '序号', name: 'num', width: 50, align: 'center', sortable: false, formatter: function (cellvalue, options, rowObject) {
                        return options.rowId;
                    }
                },
               {
                   label: '操作',
                   name: 'Oper',
                   width: 160,
                   align: 'center',
                   sortable: false,
                   formatter: function (cellvalue, options, rowObject) {
                       var html = "";
                       if (action != "show") {
                           html = "<a href=\"javascript:delRow('" + options.rowId + "')\" title=\"删除\"><i class=\"fa fa-trash-o\"></i></a>";
                       }
                       return html;
                   }
               },
               {
                   label: '姓名',
                   name: 'UserName',
                   index: 'UserName',
                   width: 200,
                   align: 'center'
               },
               {
                   label: '尺码',
                   name: 'Size',
                   index: 'Size',
                   width: 200,
                   align: 'center', editable: true, edittype: "select", editoptions: { value: selData }
               },
               {
                   label: '配备数量',
                   name: 'ShouldNum',
                   index: 'ShouldNum',
                   width: 200,
                   align: 'center', editable: true, edittype: "text", editoptions: { required: false, precision: 1, value: 0, inputEvents: { blur: function (e) { blurTextBox(e); }, keypress: function (e) { return keyPress(e); } } }
               }
            ],
            cellEdit: isPermission,
            cellsubmit: 'clientArray',
            onSelectRow: function () {
                selectedRowIndex = $('#' + this.id).getGridParam('selrow');
            },
            beforeEditCell: function (rowid, cellname, v, iRow, iCol) {
                //点击保存的时候恢复原始状态，去掉编辑列
                lastcell = iCol;
                lastrow = iRow;
            },
            gridComplete: function () {
                $('#' + this.id).setSelection(selectedRowIndex, false);
                var rowdata = $('#gridTable').jqGrid('getRowData');
                var num = 0;
                for (var i = 0; i < rowdata.length; i++) {
                    num += Number(rowdata[i].ShouldNum);
                }
                $("#ShouldNum").val(num);
                IsEdit();
            }
        });


    }

    function delRow(num) {
        var $gridTable = $('#gridTable');
        $gridTable.jqGrid('delRowData', num);

    }


    //初始化控件
    function Ini() {
        IniDropDownList();
        $.ajax({
            url: '../../LaborProtectionManage/Laborprotection/GetNameList',
            type: "GET",
            async: false,
            dataType: "json",
            success: function (result) {
                LaborList = result;
            }
        });
        $("#Name").ComboBox({
            url: "../../LaborProtectionManage/Laborprotection/GetNameList",
            id: "ID",
            text: "Name",
            description: "==请选择==",
            height: "200px",
            allowSearch: true
        }).bind("change",
            function () {
                var value = $(this).attr('data-value');
                if (value.length > 5) { //如果小于5则是随便赋值的参数 为新增
                    for (var i = 0; i < LaborList.length; i++) {
                        if (LaborList[i].ID == value) {
                            $("#No").val(LaborList[i].No);
                            $("#Model").val(LaborList[i].Model);
                            $("#Type").val(LaborList[i].Type);
                            $("#Unit").ComboBoxSetValue(LaborList[i].Unit);
                            $("#TimeNum").val(LaborList[i].TimeNum);
                            $("#TimeType").ComboBoxSetValue(LaborList[i].TimeType);
                            $("#Note").val(LaborList[i].Note);
                            isNew = 1;
                            LID = LaborList[i].ID;

                            break;
                        }
                    }
                } else {
                    no();
                    isNew = 0;
                    LID = "";
                    $("#Model").val("");
                    $("#Type").val("");
                    $("#Unit").attr("data-value", "");
                    $("#Unit").attr("data-text", "==请选择==");
                    $("#Unit div:eq(0)").text("==请选择==");
                    $("#TimeNum").val("");
                    $("#TimeType").attr("data-value", "");
                    $("#TimeType").attr("data-text", "==请选择==");
                    $("#TimeType div:eq(0)").text("==请选择==");
                    $("#Note").val("");
                }
                var isEam = false;
                for (var i = 0; i < InStocklist.length; i++) {
                    if ($(this).attr('data-text') == InStocklist[i].ItemName) {
                        $("#EamNum").val(InStocklist[i].ItemValue);
                        isEam = true;
                    }
                }
                if (!isEam) {
                    $("#EamNum").val("");
                }
                GetFiles();
                btnReadonly();
            });
        $("#TimeType").ComboBox({
            description: "==请选择==",
            height: "200px"
        });
        $("#Unit").ComboBox({
            url: "../../SystemManage/DataItemDetail/GetDataItemListJson",
            param: { EnCode: "LaborUnit" },
            id: "ItemValue",
            text: "ItemName",
            description: "==请选择==",
            height: "200px",
            allowSearch: true
        });

    }

    function no() {
        $.ajax({
            url: '../../LaborProtectionManage/Laborprotection/GetNo',
            type: "GET",
            async: false,
            dataType: "text",
            success: function (result) {
                $("#No").val(result);
            }
        });
    }

    function IniDropDownList() {
        $.ajax({
            url: '../../PersonManage/Transfer/GetOrganizeId',
            type: "GET",
            async: false,
            dataType: "text",
            success: function (result) {
                //部门
                $("#DeptId").ComboBoxTree({
                    url: top.contentPath + "/BaseManage/Department/GetOrganizeTreeJson",
                    description: "==请选择==",
                    height: "200px",
                    allowSearch: true
                }).bind("change", function () {
                    //重置岗位选择信息
                    $("#PostId").attr("data-text", "===全部===");
                    $("#PostId").attr("data-value", "");
                    $("#PostId div:eq(0)").text("===全部===");

                    //清空Grid里面的数据
                    var $gridTable = $('#gridTable');
                    var rowdata = $gridTable.jqGrid('getRowData');
                    for (var i = 0; i < rowdata.length; i++) {
                        $gridTable.jqGrid('delRowData', i + 1);
                    }

                    iniPost();
                });
            }
        });


        //岗位
        $("#PostId").ComboBox({
            description: "==请选择==",
            height: "100px",
            allowSearch: true
        });
    }

    //绑定文件
    function GetFiles() {
        $.ajax({
            url: top.contentPath + "/OccupationalHealthManage/Occupatioalstaff/GetFiles",
            data: { keyValue: LID },
            type: "GET",
            async: false,
            dataType: "JSON",
            success: function (data) {
                $('.filelist ').find("tr:not(:first)").remove();
                var falg = true;
                if (action == "show") {
                    falg = false;
                    $("#dndArea").hide();
                }
                if (LID != "") {
                    falg = false;
                    $("#dndArea").hide();

                } else if (LID == "") {
                    falg = true;
                    $("#dndArea").show();
                    
                }
                file_upload.bind(data, falg, false, LID, "uploader", true);
            }
        });
    }

    //初始化控件
    function initControl() {
        //获取表单
        if (!!keyValue) {
            $.SetForm({
                url: "../../LaborProtectionManage/Laborinfo/GetFormJson",
                param: { keyValue: keyValue },
                success: function (data) {
                    data.Name = "ceshi11";
                    $("#form1").formDeserialize(data);
                    $("#TimeNum").val(data.TimeNum);
                    ID = data.LId;//如果是修改则将id赋值给全局变量
                    LID = data.LId;
                    $("#Name").ComboBoxSetValue(data.LId);
                    iniPost();
                    $("#PostId").ComboBoxSetValue(data.PostId);
                    DorpDownSize();

                    var isEam = false;
                    for (var i = 0; i < InStocklist.length; i++) {
                        if ($("#Name").attr('data-text') == InStocklist[i].ItemName) {
                            $("#EamNum").val(InStocklist[i].ItemValue);
                            isEam = true;
                        }
                    }
                    if (!isEam) {
                        $("#EamNum").val("");
                    }
                }
            });

        } else {

            ID = '@System.Guid.NewGuid().ToString()';//如果是新增就动生成ID
            no();
        }
        DeptSelect();
        //文件上传设置
        file_upload.init({

            keyValue: ID, //业务记录Id，必须
            extensions: 'jpg,gif,bmp,png,pdf', //文件扩展名,如jpg,gif,bmp,png
            isImage: false,  //是否只是上传图片
            //fileDir: '',  //文件存储目录（如AppFile/Images），相对于Resource
            isDate: true,  //是否新建日期目录存储文件
            //fileNumLimit: 5, //允许上传的文件个数
            multiple: true //是否可以同时选择多个文件
        });
    }
    //保存表单;
    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        if ($("#TimeNum").val() != "" && ($("#TimeType").attr('data-value') == undefined || $("#TimeType").attr('data-value') == "")) {
            dialogMsg('请选择使用期限单位', 0);
            return false;
        }

        var postData = $("#form1").formSerialize(keyValue);
        $gridTable = $('#gridTable');
        $gridTable.jqGrid('saveCell', lastrow, lastcell);
        var rowdata = $gridTable.jqGrid('getRowData');
        var num = 0;
        for (var i = 0; i < rowdata.length; i++) {
            if (rowdata[i].ShouldNum == "") {
                dialogMsg(rowdata[i].UserName + '的配备数量为空，请填写配备数量', 0);
                return false;
            }
            if (!(/(^[1-9]\d*$)/.test(rowdata[i].ShouldNum))) {
                dialogMsg(rowdata[i].UserName + '的配备数量的值无效，请填写正整数', 0);
                return false;
            }
            num += Number(rowdata[i].ShouldNum);
        }
        $("#ShouldNum").val(num);
        var json = "";
        if (rowdata.length > 0) {
            var arr = [];
            var msg = "";
            var gridid = "gridTable";
            $(rowdata).each(function (i, dom) {
                dom.ID = $gridTable.find("tr").eq(i + 1).find("td[aria-describedby='" + gridid + "_ID']").eq(0).html();
                dom.UserName = $gridTable.find("tr").eq(i + 1).find("td[aria-describedby='" + gridid + "_UserName']").eq(0).html();
                dom.UserId = $gridTable.find("tr").eq(i + 1).find("td[aria-describedby='" + gridid + "_UserId']").eq(0).html();
                dom.Size = $gridTable.find("tr").eq(i + 1).find("td[aria-describedby='" + gridid + "_Size']").eq(0).html();
                dom.ShouldNum = $gridTable.find("tr").eq(i + 1).find("td[aria-describedby='" + gridid + "_ShouldNum']").eq(0).html();
                arr.push({ ID: dom.ID, UserName: dom.UserName, UserId: dom.UserId, Size: dom.Size, ShouldNum: dom.ShouldNum });
            });
            json = JSON.stringify(arr);
        }

        postData["DeptName"] = $("#DeptId").attr('data-text');
        postData["DeptCode"] = $("#DeptId").attr('data-code');
        postData["PostName"] = $("#PostId").attr('data-text');
        postData["Name"] = $("#Name").attr("data-text");
        postData["LID"] = LID;
        $.SaveForm({
            url: "../../LaborProtectionManage/Laborinfo/SaveForm?keyValue=" + keyValue,
            param: { entity: postData, json: json, ID: ID },
            loading: "正在保存数据...",
            success: function () {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
            }
        })
    }
</script>
<style>
    .form .formTitle {
        width: 30%;
    }

    .form .formValue {
        width: 70%;
    }
</style>

<div style="margin: 10px;">
    <div class="panel panel-default">
        <div class="panel-heading">
            <strong>&nbsp;&nbsp;劳动防护用品</strong>
            <span class="tools pull-right">
                <a class="fa fa-chevron-down" title="展开/收起"></a>
            </span>
        </div>
        <div class="panel-body">

            <table class="form">

                <tr>
                    <td class="formTitle">名称<font face="宋体">*</font></td>
                    <td class="formValue">
                        <div id="Name" type="select" class="ui-select" isvalid="yes" checkexpession="NotNull">

                        </div>
                    </td>
                    <td class="formTitle">编号<font face="宋体">*</font></td>
                    <td class="formValue">
                        <input id="No" type="text" readonly="readonly" class="form-control" />
                    </td>
                </tr>
                <tr>

                    <td class="formTitle">型号</td>
                    <td class="formValue">
                        <input id="Model" type="text" class="form-control" />
                    </td>
                    <td class="formTitle">类型</td>
                    <td class="formValue">
                        <select id="Type" class="ui-select">
                            <option value="">请选择或者输入</option>
                            @Html.Raw(new ERCHTMS.Busines.SystemManage.DataItemDetailBLL().GetDataItemListOption("LaborType"))
                        </select>
                    </td>
                </tr>
                <tr>

                    <td class="formTitle">使用单位/部门<font face="宋体">*</font></td>
                    <td class="formValue">
                        <div id="DeptId" type="selectTree" class="ui-select" isvalid="yes" checkexpession="NotNull">

                        </div>
                    </td>
                    <td class="formTitle">使用岗位<font face="宋体">*</font></td>
                    <td class="formValue">
                        <div id="PostId" type="select" class="ui-select" isvalid="yes" checkexpession="NotNull">

                        </div>
                    </td>
                </tr>
                <tr>

                    <td class="formTitle">劳动防护用品单位<font face="宋体">*</font></td>
                    <td class="formValue">
                        <div id="Unit" type="select" class="ui-select" isvalid="yes" checkexpession="NotNull">
                        </div>
                    </td>
                    <td class="formTitle">应配数量</td>
                    <td class="formValue">
                        <input id="ShouldNum" type="text" readonly="readonly" class="form-control" isvalid="yes" checkexpession="NotNull" />
                    </td>

                </tr>
                <tr>
                    <td class="formTitle">使用期限</td>
                    <td class="formValue">
                        <table style="width: 100%;">
                            <tr>
                                <td style="width: 60%;"><input id="TimeNum" type="text" class="form-control" isvalid="yes" checkexpession="PositiveNumOrNull" maxlength="2" /></td>
                                <td style="width: 30%;">
                                    <div id="TimeType" type="select" multiple class="ui-select">
                                        <ul>
                                            <li data-value="" class="" style="padding: 0px 5px;">==请选择==</li>
                                            <li data-value="年" title="" class="" style="padding: 0px 5px;">年</li>
                                            <li data-value="月" title="" class="" style="padding: 0px 5px;">月</li>
                                            <li data-value="日" title="" class="" style="padding: 0px 5px;">日</li>

                                        </ul>
                                    </div>
                                </td>
                            </tr>
                        </table>


                    </td>
                    <td class="formTitle">最近发放时间</td>
                    <td class="formValue">
                        <input id="RecentTime" type="text" readonly="readonly" class="form-control" />
                    </td>
                </tr>
                <tr>
                    <td class="formTitle">下次发放时间</td>
                    <td class="formValue">
                        <input id="NextTime" type="text" readonly="readonly" class="form-control" />
                    </td>
                    <td class="formTitle">发放数量</td>
                    <td class="formValue">
                        <input id="IssueNum" type="text" readonly="readonly" class="form-control" />
                    </td>
                </tr>
                <tr>
                    <td class="formTitle">库存数量</td>
                    <td class="formValue">
                        <input id="EamNum" type="text" readonly="readonly" class="form-control" />
                    </td>
                    <td class="formTitle"></td>
                    <td class="formValue"></td>
                </tr>
            </table>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            <strong>&nbsp;&nbsp;配备信息</strong>
            <span class="tools pull-right">
                <a class="fa fa-chevron-down" title="展开/收起"></a>
            </span>
        </div>
        <div class="panel-body">
            <div class="gridPanel">
                <table id="gridTable"></table>
                <div id="gridPager"></div>
            </div>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            <strong>&nbsp;&nbsp;使用说明</strong>
            <span class="tools pull-right">
                <a class="fa fa-chevron-down" title="展开/收起"></a>
            </span>
        </div>
        <div class="panel-body">
            <textarea id="Note" type="text" class="form-control" maxlength="500" style=" height: 90px;"></textarea>
        </div>
    </div>

    <div class="panel panel-default">
        <div class="panel-heading">
            <strong>&nbsp;&nbsp;附件</strong>
            <span class="tools pull-right">
                <a class="fa fa-chevron-down" title="展开/收起"></a>
            </span>
        </div>
        <div class="panel-body">
            <div id="uploader" class="uploader" style="border:1px solid #ccc; margin-top:10px; min-height:200px; margin-bottom:10px;">
                <div class="queueList">
                    <div id="dndArea" class="placeholder">
                        <div class="filePicker" style="margin-left:25px; margin-top:10px;"></div>
                    </div>
                </div>
                <div class="statusBar" style="display:none;">
                    <div class="progress">
                        <span class="text">0%</span>
                        <span class="percentage"></span>
                    </div>
                    <div class="info"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading">
            <strong>&nbsp;&nbsp;操作信息</strong>
            <span class="tools pull-right">
                <a class="fa fa-chevron-down" title="展开/收起"></a>
            </span>
        </div>
        <div class="panel-body">
            <table class="form">

                <tr>
                    <td class="formTitle">操作人</td>
                    <td class="formValue">
                        <input id="LaborOperationUserName" value="@OperatorProvider.Provider.Current().UserName" type="text" readonly="readonly" class="form-control" />
                    </td>
                    <td class="formTitle">操作时间</td>
                    <td class="formValue">
                        <input id="LaborOperationTime" type="text" value="@DateTime.Now.ToString("yyyy-MM-dd")" readonly="readonly" class="form-control" />
                    </td>
                </tr>
            </table>

        </div>
    </div>
</div>
