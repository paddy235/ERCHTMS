        postData["ENGINEERLETDEPT"] = $("#ENGINEERLETDEPTID").attr("data-text");
        postData["IsDeptAdd"] = 1;
        if ($(".ENGINEERNAME").val() != "" && $(".ENGINEERNAME").val() != null && $(".ENGINEERNAME").val() != undefined) {
            keyValue = $(".ENGINEERNAME").val();
        }
        postData["ENGINEERNAME"] = $("#ENGINEERNAME").val();
        $.SaveForm({
            url: "../../OutsourcingProject/Outsouringengineer/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function (data) {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
