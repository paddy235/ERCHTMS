@{;
    ViewBag.Title = "表单页面";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script src="~/Content/scripts/business/Common.js" type="text/javascript"></script>
<!--webUploader文件上传组件-->
<link href="~/content/scripts/plugins/webuploader/webuploader.css" rel="stylesheet" />
<link href="~/content/scripts/plugins/webuploader/webuploader-demo.css" rel="stylesheet" />
<script type="text/javascript" src="~/content/scripts/plugins/webuploader/webuploader.min.js"></script>
<script type="text/javascript" src="~/content/scripts/business/fileupload.js"></script>

<script src="~/Content/scripts/plugins/combo-select/jquery.combo.select.js"></script>
<link rel="stylesheet" href="~/Content/scripts/plugins/combo-select/combo.select.css">
<link rel="stylesheet" href="~/content/scripts/plugins/bootstrap-select/bootstrap-select.min.css" type="text/css" />
<script type="text/javascript" src="~/content/scripts/plugins/bootstrap-select/bootstrap-select.min.js"></script>

<script>
    var orgId = "@ERCHTMS.Code.OperatorProvider.Provider.Current().OrganizeId";//当前用户所属机构id

    var username = "@ERCHTMS.Code.OperatorProvider.Provider.Current().UserName";
    var userid = "@ERCHTMS.Code.OperatorProvider.Provider.Current().UserId";
    var keyType = request('keyType');
    //$(function () {

        
    //});

    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        
        if (keyType == "1") { //导出考核汇总表
            window.location.href = "../OutsourcingProject/SafetyAssessment/ExportDataTotal?time=" + $('#AUDITTIME').val() + "&deptid=" + $('#EXAMINEDEPTID').val();
        } else if (keyType == "2") { //导出外委单位考核表
            window.location.href = "../OutsourcingProject/SafetyAssessment/ExportDataOutDept?time=" + $('#AUDITTIME').val() + "&deptid=" + $('#EXAMINEDEPTID').val();
        } else if (keyType == "3") { // 导出内部单位考核表
            window.location.href = "../OutsourcingProject/SafetyAssessment/ExportDataInDept?time=" + $('#AUDITTIME').val() + "&deptid=" + $('#EXAMINEDEPTID').val();
        }

        
    }

</script>

<div id="panel3" style="margin-top:50px;margin-left:10px;">
    <div style="float:left;width:30%;margin-top:5px;">
        导出时间范围 <font face="宋体" style="color:red;">*</font>
    </div>
    <div style="float:left;width:60%" ">
        <input id="AUDITTIME" type="text" isvalid="yes" class="form-control input-wdatepicker" checkexpession="NotNull" readonly="readonly" onclick="WdatePicker({ dateFmt: 'yyyy-MM' })" />
    </div>
    <div style="float:left;width:30%;margin-top:20px;">
        提出考核部门 <font face="宋体" style="color:red;">*</font>
    </div>
    <div style="float:left;width:60%;margin-top:14px;">
        <input id="EXAMINEDEPT" type="text" class="form-control" readonly value="@ERCHTMS.Code.OperatorProvider.Provider.Current().DeptName" isvalid="yes" checkexpession="NotNull" onclick="selectDept('', 0, 0, '选择部门', document.body, 'EXAMINEDEPT,EXAMINEDEPTID,EXAMINEDEPTID')" />
        <input id="EXAMINEDEPTID" type="hidden" value="@ERCHTMS.Code.OperatorProvider.Provider.Current().DeptId" />
    </div>

</div>
