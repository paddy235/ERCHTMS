@{;
ViewBag.Title = "表单页面";
Layout = "~/Views/Shared/_Form.cshtml";
}
<script src="~/Content/scripts/business/Common.js" type="text/javascript"></script>
<!--webUploader文件上传组件-->
<link href="~/content/scripts/plugins/webuploader/webuploader.css" rel="stylesheet" />
<link href="~/content/scripts/plugins/webuploader/webuploader-demo.css" rel="stylesheet" />
<script type="text/javascript" src="~/content/scripts/plugins/webuploader/webuploader.min.js"></script>
<script type="text/javascript" src="~/content/scripts/business/fileupload.js"></script>

<script type="text/javascript">
    var keyValue = request("keyValue"); //主键 违章信息主键
    var actiontype = request("actiontype"); //操作动作
    var reseverone = request("reseverone");  //关联字段
    var resevertwo = request("resevertwo");//关联字段
    var reseverthree = request("reseverthree");//关联字段
    var lllegaldescribe = request("lllegaldescribe"); //违章描述
    var reformrequire = request("reformmeasure"); //整改措施
    var safetycheck = request("safetycheck"); //安全检查
    var applianceClass = ""; //装置类
    var curMark = ""; //标记类
    var isPrincipal = ""; //标记是否非普通员工
    var isEpiboly = ""; //标记是否承包商或分包商
    //初始方法
    $(function () {

        $(".form:eq(0)  tr:eq(8)").css("display", "none");

        $("#RESEVERONE").val(reseverone);
        $("#RESEVERTWO").val(resevertwo);
        $("#RESEVERTHREE").val(reseverthree);
        //违章描述
        if (!!lllegaldescribe) {
            $("#LLLEGALDESCRIBE").val(lllegaldescribe);
        }
        //整改措施
        if (!!reformrequire) {
            $("#REFORMREQUIRE").val(reformrequire);
        }

        $.SetForm({
            url: "../../LllegalManage/LllegalRegister/GetInitDataJson",
            success: function (data) {
                // 获取生成好的违章编码
                $("#LLLEGALNUMBER").val(data.LllegalNumber);
                $("#CREATEUSERNAME").val(data.User.UserName);
                applianceClass = data.ApplianceClass;//装置类
                curMark = data.Mark; //安全管理部门 或 装置部门判断

                isPrincipal = data.IsPrincipal;//非普通员工标记对象
                isEpiboly = data.IsEpiboly; //承包商或分包商
                //安全部门、装置部门
                if (!!curMark) {
                    $(".form:eq(0) tr:eq(3) td:eq(2)").css("display", "none");
                    $(".form:eq(0) tr:eq(3) td:eq(3)").css("display", "none");
                    //安全管理部门
                    $("#marginTopDiv").removeAttr("style");
                    $("#panel5").removeAttr("style");
                    $("#marginTopDiv").css("margin-top", "5px");
                    $("#PERSONINCHARGENAME").attr("isvalid", "yes");
                }
                else //普通员工(非安全部门、装置部门)，包含核准人员
                {
                    //一级核准，隐藏是否上报
                    if (!!isPrincipal && !isEpiboly) {
                        //一级核准人员显示考核内容
                        $("#marginTopDiv").removeAttr("style");
                        $("#panel5").removeAttr("style");
                        $("#marginTopDiv").css("margin-top", "5px");
                        $("#PERSONINCHARGENAME").attr("isvalid", "yes");
                    }
                    else {
                        //隐藏 违章曝光和是否上报安全部门
                        //是否曝光
                        $(".form:eq(0) tr:eq(3) td:eq(0)").css("display", "none");
                        $(".form:eq(0) tr:eq(3) td:eq(1)").css("display", "none");
                        //是否上报安全部门
                        $(".form:eq(0) tr:eq(3) td:eq(2)").css("display", "none");
                        $(".form:eq(0) tr:eq(3) td:eq(3)").css("display", "none");

                        HiddenValid();
                    }
                }

                //违章类型
                $("#LLLEGALTYPE").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "===全部===",
                    allowSearch: false,
                    data: data.LllegalType
                });

                //违章级别
                $("#LLLEGALLEVEL").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "===全部===",
                    allowSearch: false,
                    data: data.LllegalLevel
                });

                //流程状态
                $("#FLOWSTATE").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "===全部===",
                    allowSearch: false,
                    data: data.FlowState
                });

            }
        });

        //登记
        $("#btn_Save").click(function () {
            AcceptClick();
        });

        //提交流程
        $("#btn_Submit").click(function () {
            SubmitAction();
        });


        //通过选择类型确定是否需要填写考核内容
        $("#LLLEGALTYPE").change(function (ele) {
            ShowValid();
            var lllegalTypename = $(ele.target).attr("data-text");
            //安全主管部门
            if (curMark.indexOf("1") >= 0) {

                if (curMark.indexOf("2") < 0) {
                    //装置类,去掉必填验证
                    if (lllegalTypename.indexOf(applianceClass) >= 0) {
                        HiddenValid();
                        $("#marginTopDiv").css("display", "none");
                        $("#panel5").css("display", "none");
                        $("#PERSONINCHARGENAME").removeAttr("isvalid");
                    }
                    else {
                        //非装置类，加载所有必填项和考核信息
                        $("#marginTopDiv").removeAttr("style");
                        $("#panel5").removeAttr("style");
                        $("#marginTopDiv").css("margin-top", "5px");
                        $("#PERSONINCHARGENAME").attr("isvalid", "yes");
                    }
                }

            }
                //装置部门
            else if (curMark.indexOf("2") >= 0) {
                //装置类， 加载所有必填项和考核信息
                if (lllegalTypename.indexOf(applianceClass) >= 0) {
                    $("#marginTopDiv").removeAttr("style");
                    $("#panel5").removeAttr("style");
                    $("#marginTopDiv").css("margin-top", "5px");
                    $("#PERSONINCHARGENAME").attr("isvalid", "yes");
                }
            }
            else {
                HiddenValid();
                //一级核准人
                if (!!isPrincipal && !isEpiboly) {
                    //显示对应的考核内容，如果不上报，则直接整改，同时保存对应考核信息
                    $("#marginTopDiv").removeAttr("style");
                    $("#panel5").removeAttr("style");
                    $("#marginTopDiv").css("margin-top", "5px");
                    $("#PERSONINCHARGENAME").attr("isvalid", "yes");

                    //装置类 ，上报必选中，反之则不必
                    if (lllegalTypename.indexOf(applianceClass) >= 0) {
                        $("input[name='ISUPSAFETY']").attr("disabled", "disabled");
                        $("input[name='ISUPSAFETY']:eq(0)").prop("checked", "checked");  //上报必填

                        $("#marginTopDiv").css("display", "none");
                        $("#panel5").css("display", "none");
                        $("#PERSONINCHARGENAME").removeAttr("isvalid");
                    }
                    else {
                        //如果不上报，则显示
                        var isUpsafety = $("input[name='ISUPSAFETY']:checked").val();
                        //不上报
                        if (isUpsafety == "0") {
                            ShowValid();
                        }
                        else //上报
                        {
                            $("#marginTopDiv").css("display", "none");
                            $("#panel5").css("display", "none");
                            $("#PERSONINCHARGENAME").removeAttr("isvalid");
                        }
                        $("input[name='ISUPSAFETY']").removeAttr("disabled");
                    }
                }
                else { //普通人员 ,隐藏考核内容，待核准进行填写
                    $("#marginTopDiv").css("display", "none");
                    $("#panel5").css("display", "none");
                    $("#PERSONINCHARGENAME").removeAttr("isvalid");
                }
            }
        });

        //是否上报点击
        $("input[name='ISUPSAFETY']").click(function () {
            var checkValue = $(this).val();
            //上报
            if (checkValue == "1") {
                HiddenValid();

                $("#marginTopDiv").css("display", "none");
                $("#panel5").css("display", "none");
                $("#PERSONINCHARGENAME").removeAttr("isvalid");
            }
            else //不上报
            {
                ShowValid();

                $("#marginTopDiv").removeAttr("style");
                $("#panel5").removeAttr("style");
                $("#marginTopDiv").css("margin-top", "5px");
                $("#PERSONINCHARGENAME").attr("isvalid", "yes");
            }
        });

        //获取表单数据
        if (!!keyValue) {
            $.SetForm({
                url: "../../LllegalManage/LllegalRegister/GetFormJson",
                param: { keyValue: keyValue },
                success: function (data) {
                    //违章基本信息
                    if (null != data.baseInfo) {
                        $("#form1").formDeserialize(data.baseInfo);

                        //存在回退数据
                        if (data.baseInfo.RESEVERFOUR == "是") {
                            $(".form:eq(0)  tr:eq(8)").css("display", "");
                        }

                        $("#LLLEGALTYPE").ComboBoxSetValue(data.baseInfo.LLLEGALTYPE); //违章类型
                        $("#LLLEGALLEVEL").ComboBoxSetValue(data.baseInfo.LLLEGALLEVEL);  //违章级别

                        var lllegalType = data.baseInfo.LLLEGALTYPE;//此值为id值//

                        var lllegalTypename = ""; //此值为文本 违章类型

                        if (!!lllegalType) {
                            lllegalTypename = $("#LLLEGALTYPE").attr("data-text");
                        }

                        if (!!data.baseInfo.ISEXPOSURE) {
                            //是否曝光
                            if (data.baseInfo.ISEXPOSURE == "1") {
                                $("input[name='ISEXPOSURE']:eq(0)").prop("checked", "checked");  //是
                            }
                            else {
                                $("input[name='ISEXPOSURE']:eq(1)").prop("checked", "checked"); //否
                            }
                        }
                        //上报控制
                        if (!!data.baseInfo.ISUPSAFETY) {
                            //是否上报
                            if (data.baseInfo.ISUPSAFETY == "1") {
                                $("input[name='ISUPSAFETY']:eq(0)").prop("checked", "checked");  //是

                                /********控制必填项********/
                                HiddenValid();
                            }
                            else {
                                $("input[name='ISUPSAFETY']:eq(1)").prop("checked", "checked"); //否
                            }
                        }

                        //装置部门
                        if (lllegalTypename.indexOf(applianceClass) >= 0 && curMark.indexOf("2") >= 0) {
                            $("#marginTopDiv").removeAttr("style");
                            $("#panel5").removeAttr("style");
                            $("#marginTopDiv").css("margin-top", "5px");
                            $("#PERSONINCHARGENAME").attr("isvalid", "yes");
                        }
                            //安全主管部门，选择装置类
                        else if (lllegalTypename.indexOf(applianceClass) >= 0 && curMark.indexOf("1") >= 0) {
                            HiddenValid();
                        }
                        else {
                            //安全管理部门
                            if (curMark.indexOf("1") >= 0 && lllegalTypename.indexOf(applianceClass) < 0) {
                                $("#marginTopDiv").removeAttr("style");
                                $("#panel5").removeAttr("style");
                                $("#marginTopDiv").css("margin-top", "5px");
                                $("#PERSONINCHARGENAME").attr("isvalid", "yes");
                            }
                            else //其他部门
                            {
                                $("#marginTopDiv").css("display", "none");
                                $("#panel5").css("display", "none");
                                $("#PERSONINCHARGENAME").removeAttr("isvalid");
                            }
                        }
                    }
                    //违章整改
                    if (null != data.reformInfo) {
                        $("#form1").formDeserialize(data.reformInfo);
                        $("#REFORMID").val(data.reformInfo.ID);
                    }
                    //违章考核内容
                    if (null != data.punishInfo) {
                        $("#form1").formDeserialize(data.punishInfo);
                        $("#PUNISHID").val(data.punishInfo.ID);
                    }
                    //违章验收
                    if (null != data.acceptInfo) {
                        $("#form1").formDeserialize(data.acceptInfo);
                        $("#ACCEPTID").val(data.acceptInfo.ID);
                    }
                }
            });
        }

        if (actiontype == "view") {
            $("#btn_Save").attr("disabled", "disabled");
            $("#btn_Submit").attr("disabled", "disabled");
            $("#HaveLllegalStandard").attr("disabled", "disabled");
            $("#HaveLllegalInfo").attr("disabled", "disabled");
            $("#form1 input,textarea , .ui-select, .ui-select-text , .ui-select-option-content").each(function (ele, index) {
                $(this).attr("disabled", "disabled");
            });

            $("#btn_Save").attr("readonly", "readonly");
            $("#btn_Submit").attr("readonly", "readonly");
            $("#HaveLllegalStandard").attr("readonly", "readonly");
            $("#HaveLllegalInfo").attr("readonly", "readonly");
            $("#form1 input,textarea , .ui-select, .ui-select-text , .ui-select-option-content").each(function (ele, index) {
                $(this).attr("readonly", "readonly");
            });
        }

        //违章图片
        var curLllegalPic = $("#LLLEGALPIC").val();  //
        var isdelete = actiontype == "view" ? false : true;
        if (!curLllegalPic) {
            curLllegalPic = "@Guid.NewGuid().ToString()";
            $("#LLLEGALPIC").val(curLllegalPic);
        }
        if (actiontype != "view") {
            file_upload.init({
                keyValue: curLllegalPic, extensions: 'jpg,gif,bmp,png', isImage: true, el: '#uploader'
            });
        }
        //绑定附件
        $.ajax({
            url: '../../PublicInfoManage/ResourceFile/GetFilesByRecId',
            data: { recId: curLllegalPic },
            type: "post",
            success: function (data) {
                var objdata = eval("(" + data + ")"); //转化为对象类型
                file_upload.bind(objdata, isdelete, true, curLllegalPic, "uploader");
            }
        });

        GetApproveGrid(); //违章核准内容

        //选择已有的违章数据
        $("#HaveLllegalInfo").click(function () {
            dialogOpen({
                id: 'ChosseForm',
                title: '选择违章',
                url: '/LllegalManage/LllegalRegister/DetailList',
                width: ($(top.window).width() - 200) + "px",
                height: ($(top.window).height() - 100) + "px",
                callBack: function (iframeId) {
                    var data = new Array("LLLEGALDESCRIBE", "LLLEGALTYPE", "LLLEGALLEVEL"); //违章描述 违章类型 违章级别
                    top.document.getElementById(iframeId).contentWindow.GetObjectData(window.document.body, data);
                }
            });
        });

        //选择违章标准
        $("#HaveLllegalStandard").click(function () {
            dialogOpen({
                id: 'ChosseForm',
                title: '选择违章标准',
                url: '/LllegalStandard/LllegalStandard/Select',
                width: ($(top.window).width() - 200) + "px",
                height: ($(top.window).height() - 100) + "px",
                callBack: function (iframeId) {
                    var data = new Array("LLLEGALDESCRIBE", "LLLEGALTYPE", "LLLEGALLEVEL", "ECONOMICSPUNISH", "LLLEGALPOINT", "FIRSTECONOMICSPUNISH", "FIRSTLLLEGALPOINT", "SECONDECONOMICSPUNISH", "SECONDLLLEGALPOINT"); //违章描述 违章类型 违章级别,
                    top.document.getElementById(iframeId).contentWindow.GetObjectData(window.document.body, data);
                }
            });
        });
    });

    function HiddenValid() {
        //违章编号
        $(".form:eq(0) tr:eq(0) td:eq(0) span").remove();
        $(".form:eq(0) tr:eq(0) td:eq(1) input").removeAttr("isvalid");
        //违章人员
        $(".form:eq(0) tr:eq(0) td:eq(2) span").remove();
        $(".form:eq(0) tr:eq(0) td:eq(3) input").removeAttr("isvalid");
        //违章类型
        $(".form:eq(0) tr:eq(1) td:eq(0) span").remove();
        $(".form:eq(0) tr:eq(1) td:eq(1) div").removeAttr("isvalid");
        //违章单位
        $(".form:eq(0) tr:eq(1) td:eq(2) span").remove();
        $(".form:eq(0) tr:eq(1) td:eq(3) input").removeAttr("isvalid");
        //违章级别
        $(".form:eq(0) tr:eq(2) td:eq(0) span").remove();
        $(".form:eq(0) tr:eq(2) td:eq(1) div").removeAttr("isvalid");
        //整改要求
        $(".form:eq(0) tr:eq(7) td:eq(0) span").remove();
        $(".form:eq(0) tr:eq(7) td:eq(1) textarea").removeAttr("isvalid");
        /*整改必填验证*/
        //整改责任人
        $(".form:eq(2) tr:eq(0) td:eq(0) span").remove();
        $(".form:eq(2) tr:eq(0) td:eq(1) input").removeAttr("isvalid");
        //整改截止日期
        $(".form:eq(2) tr:eq(1) td:eq(0) span").remove();
        $(".form:eq(2) tr:eq(1) td:eq(1) input").removeAttr("isvalid");
        /*验收必填验证*/
        //验收人
        $(".form:eq(3) tr:eq(0) td:eq(0) span").remove();
        $(".form:eq(3) tr:eq(0) td:eq(1) input").removeAttr("isvalid");
        //验收日期
        $(".form:eq(3) tr:eq(1) td:eq(0) span").remove();
        $(".form:eq(3) tr:eq(1) td:eq(1) input").removeAttr("isvalid");
    }

    function ShowValid() {
        //违章编号
        $(".form:eq(0) tr:eq(0) td:eq(0)").html("违章编号<span style='color:red;'>*</span>");
        $("#LLLEGALNUMBER").attr("isvalid", "yes");
        //违章人员
        $(".form:eq(0) tr:eq(0) td:eq(2)").html("违章人员<span style='color:red;'>*</span>");
        $("#LLLEGALPERSON").attr("isvalid", "yes");
        //违章类型
        $(".form:eq(0) tr:eq(1) td:eq(0)").html("违章类型<span style='color:red;'>*</span>");
        $("#LLLEGALTYPE").attr("isvalid", "yes");
        //违章单位
        $(".form:eq(0) tr:eq(1) td:eq(2)").html("违章单位<span style='color:red;'>*</span>");
        $("#LLLEGALTEAM").attr("isvalid", "yes");
        //违章级别
        $(".form:eq(0) tr:eq(2) td:eq(0)").html("违章级别<span style='color:red;'>*</span>");
        $("#LLLEGALLEVEL").attr("isvalid", "yes");
        //整改要求
        $(".form:eq(0) tr:eq(7) td:eq(0)").html("整改要求<span style='color:red;'>*</span>");
        $("#REFORMREQUIRE").attr("isvalid", "yes");
        /*整改必填验证*/
        //整改责任人
        $(".form:eq(2) tr:eq(0) td:eq(0)").html("整改责任人<span style='color:red;'>*</span>");
        $("#REFORMPEOPLE").attr("isvalid", "yes");
        //整改截止日期
        $(".form:eq(2) tr:eq(1) td:eq(0)").html("整改截止日期<span style='color:red;'>*</span>");
        $("#REFORMDEADLINE").attr("isvalid", "yes");
        /*验收必填验证*/
        //验收人
        $(".form:eq(3) tr:eq(0) td:eq(0)").html("验收人<span style='color:red;'>*</span>");
        $("#ACCEPTPEOPLE").attr("isvalid", "yes");
        //验收日期
        $(".form:eq(3) tr:eq(1) td:eq(0)").html("验收日期<span style='color:red;'>*</span>");
        $("#ACCEPTTIME").attr("isvalid", "yes");
    }

    //核准
    function GetApproveGrid() {
        var selectedRowIndex = 0;
        var $gridTable = $('#approveGridTable');
        $gridTable.jqGrid({
            url: "../../LllegalManage/LllegalApprove/GetHistoryListJson?keyValue=" + keyValue,
            datatype: "json",
            height: $(window).height() - 500,
            autowidth: true,
            colModel: [
                {
                    label: '操作', name: 'ID', index: 'ID', width: 120, align: 'center',
                    formatter: function (cellvalue, options, rowObject) {
                        var html = "<a href=javascript:Detail('" + rowObject.ID + "')  title='查看'><i class='fa fa-eye'></i></a>";
                        return html;
                    }
                },
                { label: '核准人', name: 'APPROVEPERSON', index: 'APPROVEPERSON', width: 150, align: 'center' },
                { label: '核准单位', name: 'APPROVEDEPTNAME', index: 'APPROVEDEPTNAME', width: 120, align: 'center' },
                {
                    label: '核准结果', name: 'APPROVERESULT', index: 'APPROVERESULT', width: 150, align: 'center',
                    formatter: function (cellvalue, options, rowObject) {
                        var html = rowObject.APPROVERESULT == "1" ? "通过" : "不通过";
                        return html;
                    }
                },
                {
                    label: '核准时间', name: 'APPROVEDATE', index: 'APPROVEDATE', width: 150, align: 'center',
                    formatter: function (cellvalue, options, rowObject) {
                        var html = "";
                        if (!!rowObject.APPROVEDATE) {
                            html = rowObject.APPROVEDATE.replace("00:00:00", "");
                        }
                        return html;
                    }
                }
            ],
            pager: false,
            rowNum: "20",
            sortname: 'approvedate',
            sortorder: 'desc',
            rownumbers: true,
            shrinkToFit: false,
            gridview: true,
            ondblClickRow: function (id) {

            }
        });
    }
    //查看核准详情
    function Detail(keyValue) {
        dialogOpen({
            id: 'DetailForm',
            title: '违章核准详情',
            url: '/LllegalManage/LllegalApprove/Detail?keyValue=' + keyValue,
            width: ($(top.window).width() - 200) + "px",
            height: '680px',
            btn: null
        });
    }

    //保存表单;
    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var postData = $("#form1").formSerialize(keyValue);
        $("#btn_Save").attr("disabled", "disabled");

        //是否曝光违章
        var isExposure = $("input[name='ISEXPOSURE']:checked").val();
        postData["ISEXPOSURE"] = isExposure;

        //是否上报安全部门
        var isUpsafety = $("input[name='ISUPSAFETY']:checked").val();
        postData["ISUPSAFETY"] = isUpsafety;
        //整改ID
        postData["REFORMID"] = $("#REFORMID").val();
        //考核ID
        postData["PUNISHID"] = $("#PUNISHID").val();
        //验收ID
        postData["ACCEPTID"] = $("#ACCEPTID").val();
        $.SaveForm({
            url: "../../LllegalManage/LllegalRegister/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function () {
                if (!!safetycheck) {
                    $(top.Details.window.node).parent().next().find("input:radio").eq(1).attr("checked", "true");
                    top.Details.window.setRadioStatus(top.Details.window.node);
                }
                else {
                    $.currentIframe().$("#gridTable").trigger("reloadGrid");
                }
            },
            complete: function () {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
            }
        });
        $("#btn_Save").removeAttr("disabled");
    }

    //提交流程
    function SubmitAction() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var postData = $("#form1").formSerialize(keyValue);
        $("#btn_Submit").attr("disabled", "disabled");

        //是否曝光违章
        var isExposure = $("input[name='ISEXPOSURE']:checked").val();
        postData["ISEXPOSURE"] = isExposure;

        //是否上报安全部门
        var isUpsafety = $("input[name='ISUPSAFETY']:checked").val();
        postData["ISUPSAFETY"] = isUpsafety;

        //整改ID
        postData["REFORMID"] = $("#REFORMID").val();
        //考核ID
        postData["PUNISHID"] = $("#PUNISHID").val();
        //验收ID
        postData["ACCEPTID"] = $("#ACCEPTID").val();

        $.SaveForm({
            url: "../../LllegalManage/LllegalRegister/SubmitForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function () {
                if (!!safetycheck) {
                    $(top.Details.window.node).parent().next().find("input:radio").eq(1).attr("checked", "true");
                     top.Details.window.setRadioStatus(top.Details.window.node);
                }
                else {
                    $.currentIframe().$("#gridTable").trigger("reloadGrid");
                }
            }
        });
        $("#btn_Submit").removeAttr("disabled");
    }
</script>

