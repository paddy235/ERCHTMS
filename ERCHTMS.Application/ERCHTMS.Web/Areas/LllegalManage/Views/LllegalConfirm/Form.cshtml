                            <input id="CONFIRMPEOPLE" class="form-control" placeholder="请选择确认人" errormsg="确认人" isvalid="yes" checkexpession="NotNull" readonly onclick="selectDifferentUser('', 0, 2, window.document.body, 'CONFIRMPEOPLE,,CONFIRMPEOPLEID,,CONFIRMDEPTCODE,CONFIRMDEPTNAME', 'xxx', 'xxx');" />
                            <input id="CONFIRMPEOPLEID" type="hidden" />
                            <input id="CONFIRMID" type="hidden" />
