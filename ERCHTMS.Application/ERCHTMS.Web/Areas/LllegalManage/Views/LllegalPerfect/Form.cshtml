    //保存表单;
    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var postData = $("#form1").formSerialize(keyValue);
        $("#btn_Save").attr("disabled", "disabled");
        //所属单位
        postData["BELONGDEPART"] = $("#BELONGDEPART").text();
        //是否省公司验收
        var isGrpAccept = $("input[name='ISGRPACCEPT']:checked").val();
        postData["ISGRPACCEPT"] = isGrpAccept;
        //是否曝光违章
        var isExposure = $("input[name='ISEXPOSURE']:checked").val();
        postData["ISEXPOSURE"] = isExposure;

        //是否上报安全部门
        var isUpsafety = $("input[name='ISUPSAFETY']:checked").val();
        postData["ISUPSAFETY"] = isUpsafety;
        //整改ID
        postData["REFORMID"] = $("#REFORMID").val();
        //验收ID
        postData["ACCEPTID"] = $("#ACCEPTID").val();

        //考核信息
        var relevanceData = $('#examinGridTable').jqGrid("getRowData");
        if (!!relevanceData) {
            $(relevanceData).each(function (index, ele) {
                ele.oper = "";
            });
        }
        postData["RELEVANCEDATA"] = JSON.stringify(relevanceData);
        //保存
        $.SaveForm({
            url: "../../LllegalManage/LllegalRegister/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function () {
                if (!!safetycheck) {
                    $(top.Details.window.node).parent().next().find("input:radio").eq(1).attr("checked", "true");
                    top.Details.window.setRadioStatus(top.Details.window.node);
                    $.currentIframe().$("#gridTable").trigger("reloadGrid");
                }
                else {
                    $.currentIframe().$("#gridTable").trigger("reloadGrid");
                }
                $("#btn_Save").removeAttr("disabled");
            },
            complete: function () {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
                $("#btn_Save").removeAttr("disabled");
            }
        });
    }
    //提交流程
    function SubmitAction() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var subCount = 0;
        if (!!ControlPicMustUpload) {
            var array = new Array();
            if (ControlPicMustUpload.indexOf("LLLEGALPIC") >= 0) {
                array.push({ key: "LLLEGALPIC", value: $("#LLLEGALPIC").val() });
            }
            if (array.length > 0) {
                //绑定附件
                $.ajax({
                    url: '../../PublicInfoManage/ResourceFile/GetFilesByJsonData',
                    data: { jsonData: JSON.stringify(array) },
                    type: "post",
                    success: function (data) {
                        var yesSubmit = "";
                        if (!!data) {
                            var result = eval("(" + data + ")");
                            $(result).each(function (i, ele) {
                                var key = ele.key;
                                switch (key) {
                                    case "LLLEGALPIC":
                                        yesSubmit += parseInt(ele.value) > 0 ? "1" : ""; //违章图片
                                        break;
                                }
                            });
                            var errorMsg = "请上传";
                            if (ControlPicMustUpload.indexOf("LLLEGALPIC") >= 0) {
                                if (yesSubmit.indexOf("1") >= 0) {
                                    subCount += 1;
                                }
                                else {
                                    errorMsg += "违章图片,";
                                }
                            }
                            if (subCount == result.length) {
                                SubmitData();
                            }
                            else {
                                if (subCount < result.length) {
                                    errorMsg = errorMsg.substring(0, errorMsg.length - 1);
                                }
                                dialogMsg(errorMsg, 0);
                            }
                        }
                    }
                });
            }
        }
        else {
            SubmitData();
        }
    }


    function SubmitData()
    {
        var postData = $("#form1").formSerialize(keyValue);
        //所属单位
        postData["BELONGDEPART"] = $("#BELONGDEPART").text();
        //是否省公司验收
        var isGrpAccept = $("input[name='ISGRPACCEPT']:checked").val();
        postData["ISGRPACCEPT"] = isGrpAccept;
        //是否曝光违章
        var isExposure = $("input[name='ISEXPOSURE']:checked").val();
        postData["ISEXPOSURE"] = isExposure;

        //是否上报安全部门
        var isUpsafety = $("input[name='ISUPSAFETY']:checked").val();
        postData["ISUPSAFETY"] = isUpsafety;

        //整改ID
        postData["REFORMID"] = $("#REFORMID").val();
        //验收ID
        postData["ACCEPTID"] = $("#ACCEPTID").val();

        var crossNum = 0;
        //考核信息
        var relevanceData = $('#examinGridTable').jqGrid("getRowData");
        if (!!relevanceData) {
            $(relevanceData).each(function (index, ele) {
                if (!ele.PERSONINCHARGENAME) {
                    crossNum += 1;
                }
                ele.oper = "";
            });
        }
        postData["RELEVANCEDATA"] = JSON.stringify(relevanceData);
        if (crossNum > 0) {
            dialogMsg('考核人员/单位不能为空！', 0);
        }
        else {
            $("#btn_Submit").attr("disabled", "disabled");
            //提交
            $.SaveForm({
                url: "../../LllegalManage/LllegalRegister/SubmitForm?keyValue=" + keyValue,
                param: postData,
                loading: "正在保存数据...",
                success: function () {
                    if (!!safetycheck) {
                        $(top.Details.window.node).parent().next().find("input:radio").eq(1).attr("checked", "true");
                        top.Details.window.setRadioStatus(top.Details.window.node);
                        $.currentIframe().$("#gridTable").trigger("reloadGrid");
                    }
                    else {
                        $.currentIframe().$("#gridTable").trigger("reloadGrid");
                    }
                    $("#btn_Submit").removeAttr("disabled");
                },
                complete: function () {
                    $("#btn_Submit").removeAttr("disabled");
                }
            });
        }
    }
</script>

