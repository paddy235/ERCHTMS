                        var html = "";
                        if (actiontype != "view") {
                            html = "<a href=javascript:editaward()  title='修改'><i class='fa fa-pencil-square-o'></i></a>";
                            html += "<a href=javascript:removeaward()  title='删除'><i class='fa fa-trash-o'></i></a>";
                        }
                        return html;
                    }
                },
                {
                    label: '奖励人员', name: 'USERNAME', index: 'USERNAME', width: 150, align: 'center'
                },
                {
                    label: '奖励人员单位', name: 'DEPTNAME', index: 'DEPTNAME', width: 200, align: 'center'
                },
                {
                    label: '奖励积分(分)', name: 'POINTS', index: 'POINTS', width: 150, align: 'center'
                },
                {
                    label: '奖励金额(元)', name: 'MONEY', index: 'MONEY', width: 150, align: 'center'
                }
            ],
            sortname: 'CREATEDATE',
            rownumbers: true,
            shrinkToFit: false,
            gridview: true,
            onSelectRow: function () {
                selectedRowIndex = $("#" + this.id).getGridParam('selrow');
            },
            gridComplete: function () {
                $("#" + this.id).setSelection(selectedRowIndex, false);
            }
        });

    }

    //上传文件
    function uploadify(objId) {
        var folderId = "";
        var fileType = 'bmp,ico,gif,jpeg,jpg,png,psd';
        var isImage = true;
        switch (objId) {
            case "uploader": //违章图片
                folderId = $("#LLLEGALPIC").val();
                if (!folderId) {
                    folderId = "@Guid.NewGuid().ToString()";
                    $("#LLLEGALPIC").val(folderId);
                }
                if (actiontype != "view") {
                    file_upload.init({
                        keyValue: folderId, extensions: fileType, isImage: isImage, el: '#uploader'
                    });
                }
                break;
            case "uploader_reformpic": //整改图片
                folderId = $("#REFORMPIC").val();
                if (!folderId) {
                    folderId = "@Guid.NewGuid().ToString()";
                    $("#REFORMPIC").val(folderId);
                }
                if (actiontype != "view") {
                    file_upload.init({
                        keyValue: folderId, extensions: fileType, isImage: isImage, el: '#uploader_reformpic'
                    });
                }
                break;
            case "uploader_attachment": //整改附件
                folderId = $("#REFORMATTACHMENT").val();
                isImage = false;
                fileType = 'bmp,ico,gif,jpeg,jpg,png,psd,zip,rar,doc,docx,xls,xlsx,pdf,txt,chm';
                if (!folderId) {
                    folderId = "@Guid.NewGuid().ToString()";
                    $("#REFORMATTACHMENT").val(folderId);
                }
                if (actiontype != "view") {
                    file_upload.init({
                        keyValue: folderId, extensions: fileType, isImage: isImage, el: '#uploader_attachment'
                    });
                }
                break;
            case "uploader_acceptpic":  //验收图片
                folderId = $("#ACCEPTPIC").val();
                if (!folderId) {
                    folderId = "@Guid.NewGuid().ToString()";
                    $("#ACCEPTPIC").val(folderId);
                }
                if (actiontype != "view") {
                    file_upload.init({
                        keyValue: folderId, extensions: fileType, isImage: isImage, el: '#uploader_acceptpic'
                    });
                }
                break;
        }

        $.ajax({
            url: '../../PublicInfoManage/ResourceFile/GetFilesByRecId',
            data: { recId: folderId },
            type: "post",
            success: function (data) {
                var isdelete = actiontype == "view" ? false : true;
                var objdata = eval("(" + data + ")"); //转化为对象类型
                file_upload.bind(objdata, isdelete, isImage, folderId, objId);
            }
        });
    }

    //提交流程
    function SubmitAction() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var subCount = 0;
        if (!!ControlPicMustUpload) {
            if (ControlPicMustUpload.indexOf("LLLEGALPIC") >= 0) {
                var array = new Array();
                if (ControlPicMustUpload.indexOf("LLLEGALPIC") >= 0) {
                    array.push({ key: "LLLEGALPIC", value: $("#LLLEGALPIC").val() });
                }
                if (ControlPicMustUpload.indexOf("REFORMPIC") >= 0) {
                    array.push({ key: "REFORMPIC", value: $("#REFORMPIC").val() });
                }
                if (ControlPicMustUpload.indexOf("ACCEPTPIC") >= 0) {
                    array.push({ key: "ACCEPTPIC", value: $("#ACCEPTPIC").val() });
                }
                if (array.length > 0) {
                    //绑定附件
                    $.ajax({
                        url: '../../PublicInfoManage/ResourceFile/GetFilesByJsonData',
                        data: { jsonData: JSON.stringify(array) },
                        type: "post",
                        success: function (data) {
                            var yesSubmit = "";
                            if (!!data) {
                                var result = eval("(" + data + ")");
                                $(result).each(function (i, ele) {
                                    var key = ele.key;
                                    switch (key) {
                                        case "LLLEGALPIC":
                                            yesSubmit += parseInt(ele.value) > 0 ? "1" : ""; //违章图片
                                            break;
                                        case "REFORMPIC":
                                            yesSubmit += parseInt(ele.value) > 0 ? "2" : ""; //违章整改图片
                                            break;
                                        case "ACCEPTPIC":
                                            yesSubmit += parseInt(ele.value) > 0 ? "3" : ""; //违章验收图片
                                            break;
                                    }
                                });

                                var errorMsg = "请上传";
                                if (ControlPicMustUpload.indexOf("LLLEGALPIC") >= 0) {
                                    if (yesSubmit.indexOf("1") >= 0) {
                                        subCount += 1;
                                    }
                                    else {
                                        errorMsg += "违章图片,";
                                    }
                                }
                                if (ControlPicMustUpload.indexOf("REFORMPIC") >= 0) {
                                    if (yesSubmit.indexOf("2") >= 0) {
                                        subCount += 1;
                                    }
                                    else {
                                        errorMsg += "违章整改图片,";
                                    }
                                }
                                if (ControlPicMustUpload.indexOf("ACCEPTPIC") >= 0) {
                                    if (yesSubmit.indexOf("3") >= 0) {
                                        subCount += 1;
                                    }
                                    else {
                                        errorMsg += "违章验收图片,";
                                    }
                                }
                                if (subCount == result.length) {
                                    SubmitData();
                                }
                                else {
                                    if (subCount < result.length) {
                                        errorMsg = errorMsg.substring(0, errorMsg.length - 1);
                                    }
                                    dialogMsg(errorMsg, 0);
                                }
                            }
                        }
                    });
                }
            }
        }
        else {
            SubmitData();
        }
    }

    function SubmitData()
    {
        var postData = $("#form1").formSerialize(keyValue);
        //核准结果
        var approveResult = $("input[name='APPROVERESULT']:checked").val();
        postData["APPROVERESULT"] = approveResult;
        //整改完成情况
        var reformStatus = $("input[name='REFORMSTATUS']:checked").val();
        postData["REFORMSTATUS"] = reformStatus;
        //验收情况
        var acceptResult = $("input[name='ACCEPTRESULT']:checked").val();
        postData["ACCEPTRESULT"] = acceptResult;
        //是否曝光违章
        var isExposure = $("input[name='ISEXPOSURE']:checked").val();
        postData["ISEXPOSURE"] = isExposure;
        //考核信息
        var crossNum = 0;
        var relevanceData = $('#examinGridTable').jqGrid("getRowData");
        if (!!relevanceData) {
            $(relevanceData).each(function (index, ele) {
                if (!ele.PERSONINCHARGENAME) {
                    crossNum += 1;
                }
                ele.oper = "";
            });
        }
        postData["RELEVANCEDATA"] = JSON.stringify(relevanceData);
        //奖励信息
        var awardData = $('#awardGridTable').jqGrid("getRowData");
        if (!!awardData) {
            $(awardData).each(function (index, ele) {
                ele.oper = "";
            });
        }
        postData["AWARDDATA"] = JSON.stringify(awardData);

        if (crossNum > 0) {
            dialogMsg('考核人员/单位不能为空！', 0);
        }
        else {
            if (!!$("#LLLEGALTEAMCODE").val()) {
                $("#btn_Submit").attr("disabled", "disabled");
                $.SaveForm({
                    url: "../../LllegalManage/LllegalRegister/OneSubmitForm?keyValue=" + keyValue,
                    param: postData,
                    loading: "正在保存数据...",
                    success: function () {
                        $.currentIframe().$("#gridTable").trigger("reloadGrid");
                        $("#btn_Submit").removeAttr("disabled");
                    },
                    complete: function () {
                        $("#btn_Submit").removeAttr("disabled");
                    }
                });
            } else {
                dialogMsg('请直接或输入后选择违章单位！', 0);
            }
        }
    }
</script>


