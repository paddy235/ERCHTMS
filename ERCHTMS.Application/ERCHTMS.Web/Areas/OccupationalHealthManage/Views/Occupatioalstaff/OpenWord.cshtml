@{
    ViewBag.Title = "查看文档";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var fileUrl = request("fileUrl");
    var topUrl = request("topUrl");
    $(function () {
        InitialPage();

    });
    //初始化数据
    function InitialPage() {
        //记载文档
        OpenDocument(topUrl + '/OccupationalHealthManage/Occupatioalstaff/GetWord?fileUrl=' + fileUrl);
    }
    function OpenDocument(strUrl) {

        $.get(strUrl, { param2: "" }, function (data) {
            $("#EmerDetail").html(data);
        });
    }

</script>
<link href="~/Content/static/css/help.css" rel="stylesheet" />
<link href="~/Content/static/css/extend.css" rel="stylesheet" />

<div class="main-content">
    <div class="container-fluid">

        <div class="row">
            <div class="col-md-12 my-title">
                <div class="pull-left" id="ShowT">

                </div>
                <div class="pull-right">
                </div>
            </div>
        </div>

        <div class="bd-fdfdfd pd-20 main" id="EmerDetail" style="height:800px">

        </div>

    </div>
</div>


