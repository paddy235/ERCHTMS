@{;
    ViewBag.Title = "表单页面";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = request('keyValue');
    $(function () {
        initControl();
    });
    //初始化控件
    function initControl() {
        //获取表单
        if (!!keyValue) {
            $.SetForm({
                url: "../../CarManage/Visitcar/GetFormJson",
                param: { keyValue: keyValue },
                success: function (data) {
                    $("#form1").formDeserialize(data);
                }
            })
        }
    }
    //保存表单;
    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var postData = $("#form1").formSerialize(keyValue);
        $.SaveForm({
            url: "../../CarManage/Visitcar/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function () {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
            }
        })
    }
</script>
<div style="margin-top: 20px; margin-right: 30px;">
    <table class="form">
        <tr>
            <td class="formTitle">主键</td>
            <td class="formValue">
                <input id="ID" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">入厂时间</td>
            <td class="formValue">
                <input id="INTIME" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">出厂时间</td>
            <td class="formValue">
                <input id="OUTTIME" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">驾驶人</td>
            <td class="formValue">
                <input id="DIRVER" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">驾驶证图片地址</td>
            <td class="formValue">
                <input id="DRIVERLICENSEURL" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">定位设备名称</td>
            <td class="formValue">
                <input id="GPSNAME" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">车牌号</td>
            <td class="formValue">
                <input id="CARNO" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">拜访对象</td>
            <td class="formValue">
                <input id="VISITUSER" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">拜访对象电话</td>
            <td class="formValue">
                <input id="VISITUSERPHONE" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">司机电话</td>
            <td class="formValue">
                <input id="PHONE" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">拜访人数</td>
            <td class="formValue">
                <input id="ACCOMPANYINGNUMBER" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">行驶证图片地址</td>
            <td class="formValue">
                <input id="DRIVINGLICENSEURL" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">随行人员</td>
            <td class="formValue">
                <input id="ACCOMPANYINGPERSON" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">拜访部门</td>
            <td class="formValue">
                <input id="VISITDEPT" type="text" class="form-control"  />
            </td>
        </tr>
       
    </table>
</div>
