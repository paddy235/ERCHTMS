@{;
    ViewBag.Title = "表单页面";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<script>
    var keyValue = request('keyValue');
    $(function () {
        initControl();
    });
    //初始化控件
    function initControl() {
        //获取表单
        if (!!keyValue) {
            $.SetForm({
                url: "../../CarManage/Hikaccessaurhority/GetFormJson",
                param: { keyValue: keyValue },
                success: function (data) {
                    $("#form1").formDeserialize(data);
                }
            })
        }
    }
    //保存表单;
    function AcceptClick() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var postData = $("#form1").formSerialize(keyValue);
        $.SaveForm({
            url: "../../CarManage/Hikaccessaurhority/SaveForm?keyValue=" + keyValue,
            param: postData,
            loading: "正在保存数据...",
            success: function () {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
            }
        })
    }
</script>
<div style="margin-top: 20px; margin-right: 30px;">
    <table class="form">
        <tr>
            <td class="formTitle">主键</td>
            <td class="formValue">
                <input id="ID" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">权限结束时间</td>
            <td class="formValue">
                <input id="ENDTIME" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">设备在安防平台中的ID</td>
            <td class="formValue">
                <input id="HIKID" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">进出类型 0表示进门设备 1表示出门设备</td>
            <td class="formValue">
                <input id="OUTTYPE" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">0为部门 1为用户</td>
            <td class="formValue">
                <input id="TYPE" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">门禁控制器ID</td>
            <td class="formValue">
                <input id="PARENTID" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">权限开始时间</td>
            <td class="formValue">
                <input id="STARTTIME" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">创建人所属部门</td>
            <td class="formValue">
                <input id="CREATEUSERDEPTCODE" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">设备名称</td>
            <td class="formValue">
                <input id="DEVICENAME" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">创建人ID</td>
            <td class="formValue">
                <input id="CREATEUSERID" type="text" class="form-control"  />
            </td>
        </tr>
        <tr>
            <td class="formTitle">所属部门或用户关联ID</td>
            <td class="formValue">
                <input id="RID" type="text" class="form-control"  />
            </td>
        </tr>
       
    </table>
</div>
