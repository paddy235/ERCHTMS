@{;
ViewBag.Title = "表单页面";
Layout = "~/Views/Shared/_Form.cshtml";
}
<script src="~/Content/scripts/business/Common.js" type="text/javascript"></script>
<script>
    var keyValue = request("keyValue"); //主键 隐患信息主键
    var actiontype = request("actiontype");
    $(function () {

        $(".center-Panel").css({ "width": "100%", "height": ($(top.window).height() - 160) + "px", "padding-top": "38px" });

        initControl();
        GetGrid();

        //查看下
        if (actiontype == "view") {
            $("#btn_UpSubmit").attr("disabled", "disabled");
            $("#btn_Submit").attr("disabled", "disabled");
            $("#lr-uploadify").attr("disabled", "disabled");
            $("#form1 input,textarea , .ui-select, .ui-select-text , .ui-select-option-content").each(function (ele, index) {
                $(this).attr("disabled", "disabled");
            });
        }

        $(".form-control").focus(function () {
            $(".poptip").remove();
        });

        $.ajax({
            type: "get",
            url: "../../HiddenTroubleManage/HTBaseInfo/QuerySafetyRole",
            success: function (data) {
                //非安全管理员或指定人员  整改信息及验收信息非必填
                if (parseInt(data) == 0) {
                    $("#btn_UpSubmit").remove();
                }
            }
        });

        $.ajax({
            type: "get",
            url: "../../HiddenTroubleManage/HTApproval/IsAssignDepartment",
            success: function (data) {
                //指定部门 移除上报按钮
                if (parseInt(data) == 1) {
                    $("#btn_UpSubmit").remove();
                }
            }
        });
    });


    //获取列表
    function GetGrid() {
        var folderId = $("#HIDPHOTO").val(); //隐患图片
        var selectedRowIndex = 0;
        var $gridTable = $('#picgridTable');
        $gridTable.jqGrid({
            url: "../../PublicInfoManage/ResourceFile/GetImageListJsonByFolder?folderId=" + folderId,
            datatype: "json",
            height: $(window).height()/2 - 200,
            autowidth: true,
            colModel: [
                { label: '标记', name: 'FilePath', hidden: true },
                { label: 'FolderId', name: 'FolderId', hidden: true },
                { label: 'FileType', name: 'FileType', hidden: true },
                {
                    label: '文件名', width: $(".profile-content").width() - 350, align: 'center',
                    formatter: function (cellvalue, options, rowObject) {
                        return "<div style='cursor:pointer;'><div style='float: left;'><img src='" + top.contentPath + "/Content/images/filetype/" + rowObject.FileType +
                            ".png' style='width:35px;height:35px;padding:5px;margin-left:-7px;margin-right:5px;' /></div>" +
                            "<div style='float: left;line-height:35px;'><a href='" + top.contentPath + rowObject.FilePath.replace("~", "") + "' target='_blank'>" + rowObject.FileName + "</a></div></div>";
                    }
                },
            ],
            pager: false,
            rowNum: "10",
            rownumbers: true,
            shrinkToFit: false,
            gridview: true,
            ondblClickRow: function (id) {

            }
        });
    }

    //初始化控件
    function initControl() {

        var curDate = "@DateTime.Now.ToString("yyyy-MM-dd")";
        //默认给当前时间
        $("#APPROVALDATE").val(curDate);

        //提交流程
        $("#btn_Submit").click(function () {
            SubmitAction();
        });

        //上报
        $("#btn_UpSubmit").click(function () {
            UpSubmitAction();
        });

        $.SetForm({
            url: "../../HiddenTroubleManage/HTBaseInfo/GetInitDataJson",
            success: function (data) {
                //检查类型
                $("#CHECKTYPE").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "==请选择==",
                    allowSearch: false,
                    data: data.CheckType
                });
                //隐患类型
                $("#HIDTYPE").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "==请选择==",
                    allowSearch: false,
                    data: data.HidType
                });
                //隐患级别
                $("#HIDRANK").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "==请选择==",
                    allowSearch: false,
                    data: data.HidRank,
                });
                //培训模板
                $("#TRAINTEMPLATENAME").ComboBox({
                    id: "ItemDetailId",
                    text: "ItemName",
                    description: "==请选择==",
                    allowSearch: false,
                    data: data.TrainTemplateName,
                });
            }
        });


        //隐藏回退原因
        $(".form tr:eq(24)").css("display", "none");
        $(".form tr:eq(24) td").each(function (ele, index) {
            $(this).css("display", "none");
        });

        if ($("#HIDRANK")[0].innerText.indexOf("重大") < 0) {
            //隐藏重大隐患填项
            for (var i = 13; i <= 18; i++) {
                $(".form tr:eq(" + i + ")").css("display", "none");
                $(".form tr:eq(" + i + ") td").each(function (ele, index) {
                    $(this).css("display", "none");
                });
            }
        }

        $("#HIDRANK").change(function (ele) {
            var rankName = $(ele.target).attr("data-text");
            if (rankName.indexOf("重大") > 0) {
                for (var i = 13; i <= 18; i++) {
                    $(".form tr:eq(" + i + ")").removeAttr("style");
                    $(".form tr:eq(" + i + ") td").each(function (ele, index) {
                        $(this).removeAttr("style");
                    });
                }
            }
            else {
                for (var i = 13; i <= 18; i++) {
                    $(".form tr:eq(" + i + ")").css("display", "none");
                    $(".form tr:eq(" + i + ") td").each(function (ele, index) {
                        $(this).css("display", "none");
                    });
                }
            }
        });



        //判断是否违章
        var chkVal = $("input[name='ISBREAKRULE']:checked").val();

        if (chkVal == "0") {
            $(".form tr:eq(9)").css("display", "none");
            $(".form tr:eq(9) td").each(function (ele, index) {
                $(this).css("display", "none");
            });

            $(".form tr:eq(8) td").each(function (index, ele) {
                if (index > 1) {
                    $(this).css("display", "none");
                }
            });
        }
        //判断是否评估通过
        var approvalVal = $("input[name='APPROVALRESULT']:checked").val();
        if (approvalVal == "1") {
            $(".form tr:eq(2)").css("display", "none");
            $(".form tr:eq(2) td").each(function (ele, index) {
                $(this).css("display", "none");
            });
        }

        //评估是否通过验证
        $("input[name='APPROVALRESULT']").click(function () {
            var checkValue = $(this).val();
            //核查通过
            if (checkValue == "1") {
                $(".form tr:eq(2)").css("display", "none");
                $(".form tr:eq(2) td").each(function (ele, index) {
                    $(this).css("display", "none");
                });

                $("#CHANGEPERSONNAME").attr("isvalid", "yes");

                $("#CHANGEDEADINE").attr("isvalid", "yes");

                $("#PLANMANAGECAPITAL").attr("isvalid", "yes");

                $("#ACCEPTPERSONNAME").attr("isvalid", "yes");

                $("#ACCEPTDATE").attr("isvalid", "yes");

                $(".form:eq(2) tr:eq(0) td:eq(0)").html("整改责任人<span style='color:red;'>*</span>"); //整改责任人
                $(".form:eq(2) tr:eq(2) td:eq(0)").html("整改截至时间<span style='color:red;'>*</span>"); //整改截至时间
                $(".form:eq(2) tr:eq(2) td:eq(2)").html("计划治理资金(元)<span style='color:red;'>*</span>"); //计划治理资金(元)

                $(".form:eq(3) tr:eq(0) td:eq(0)").html("验收人<span style='color:red;'>*</span>"); //验收人
                $(".form:eq(3) tr:eq(1) td:eq(0)").html("验收日期<span style='color:red;'>*</span>"); //验收日期


                $("#btn_UpSubmit").removeAttr("disabled"); //移除
            }
            else {//核查不通过
                $(".form tr:eq(2)").removeAttr("style");
                $(".form tr:eq(2) td").each(function (ele, index) {
                    $(this).removeAttr("style");
                });

                $("#CHANGEPERSONNAME").removeAttr("isvalid");

                $("#CHANGEDEADINE").removeAttr("isvalid");

                $("#PLANMANAGECAPITAL").removeAttr("isvalid");

                $("#ACCEPTPERSONNAME").removeAttr("isvalid");

                $("#ACCEPTDATE").removeAttr("isvalid");

                $(".form:eq(2) tr:eq(0) span").remove(); //整改责任人
                $(".form:eq(2) tr:eq(2) span").remove(); //整改截至时间  计划治理资金(元)

                $(".form:eq(3) tr:eq(0) span").remove(); //验收人
                $(".form:eq(3) tr:eq(1) span").remove(); //验收日期

                $("#btn_UpSubmit").attr("disabled", "disabled"); //添加
            }
        });

        //获取表单
        if (!!keyValue) {
            $.SetForm({
                url: "../../HiddenTroubleManage/HTBaseInfo/GetFormJson",
                param: { keyValue: keyValue },
                success: function (data) {

                    //隐患整改
                    if (null != data.changeInfo) {
                        $("#CHANGEID").val(data.changeInfo.ID);
                        $("#form1").formDeserialize(data.changeInfo);

                        //回退原因
                        if (null != data.changeInfo.BACKREASON && "" != data.changeInfo.BACKREASON) {
                            $(".form tr:eq(24)").removeAttr("style");
                            $(".form tr:eq(24) td").each(function (index, ele) {
                                $(this).removeAttr("style");
                            });
                        }
                    }
                    //隐患验收
                    if (null != data.acceptInfo) {
                        $("#ACCEPTID").val(data.acceptInfo.ID);
                        $("#form1").formDeserialize(data.acceptInfo);
                    }

                    if (null != data.baseInfo) {
                        $("#form1").formDeserialize(data.baseInfo);

                        $("#CHECKTYPE").ComboBoxSetValue(data.baseInfo.CHECKTYPE);
                        $("#HIDRANK").ComboBoxSetValue(data.baseInfo.HIDRANK);
                        $("#HIDTYPE").ComboBoxSetValue(data.baseInfo.HIDTYPE);
                        $("#TRAINTEMPLATENAME").ComboBoxSetValue(data.baseInfo.TRAINTEMPLATENAME);


                        //是否违章
                        if (data.baseInfo.ISBREAKRULE == "1") {
                            $("input[name='ISBREAKRULE']:eq(0)").prop("checked", "checked");
                        }
                        else {
                            $("input[name='ISBREAKRULE']:eq(1)").prop("checked", "checked");
                        }

                        //是否挂牌督办
                        if (data.baseInfo.ISGETAFTER == "1") {
                            $("input[name='ISGETAFTER']:eq(0)").prop("checked", "checked");
                        }
                        else {
                            $("input[name='ISGETAFTER']:eq(1)").prop("checked", "checked");
                        }

                        //是否曝光
                        if (data.baseInfo.EXPOSURESTATE == "1") {
                            $("input[name='EXPOSURESTATE']:eq(0)").prop("checked", "checked");
                        }
                        else {
                            $("input[name='EXPOSURESTATE']:eq(1)").prop("checked", "checked");
                        }
                    }

                    //当前用户
                    if (null != data.userInfo) {
                        $("#APPROVALPERSON").val(data.userInfo.UserId);
                        $("#APPROVALPERSONNAME").val(data.userInfo.UserName);
                        $("#APPROVALDEPARTCODE").val(data.userInfo.DeptCode);
                        $("#APPROVALDEPARTNAME").val(data.userInfo.DeptName);
                    }

                    if ($("#HIDRANK")[0].innerText.indexOf("重大") > 0) {
                        //隐藏重大隐患填项
                        for (var i = 13; i <= 18; i++) {
                            $(".form tr:eq(" + i + ")").removeAttr("style");
                            $(".form tr:eq(" + i + ") td").each(function (ele, index) {
                                $(this).removeAttr("style");
                            });
                        }
                    }
                }
            })
        }
    }

    //提交流程
    function SubmitAction() {
        if (!$('#form1').Validform()) {
            return false;
        }
        var postData = $("#form1").formSerialize(keyValue);
        var account = "";

        var approvalVal = $("input[name='APPROVALRESULT']:checked").val();

        postData["APPROVALRESULT"] = approvalVal;

        //是否曝光
        var exposurestateVal = $("input[name='EXPOSURESTATE']:checked").val();

        postData["EXPOSURESTATE"] = exposurestateVal;

        var userID = ""; //
        //核查通过
        if (approvalVal == "1") {
            userID = $("#CHANGEPERSON").val();
        }
        else//核查不通过
        {
            userID = $("#CREATEUSERID").val();
        }

        $.SetForm({
            url: "../../BaseManage/User/GetFormJson",
            param: { keyValue: userID },
            success: function (data) {
                if (data != null) {
                    account = data.Account;
                    //提交流程
                    $.SaveForm({
                        url: "../../HiddenTroubleManage/HTApproval/SaveForm?keyValue=" + keyValue + "&submitUser=" + account + "&isUpSubmit=0",
                        param: postData,
                        loading: "正在保存数据...",
                        success: function () {
                            $.currentIframe().$("#gridTable").trigger("reloadGrid");
                        }
                    });
                }
                else {
                    dialogMsg('评估关联用户异常,无法提交,请联系管理员!', 0);
                }
            }
        });
    }

    //隐患上报评估
    function UpSubmitAction() {
        var postData = $("#form1").formSerialize(keyValue);
        var approvalVal = $("input[name='APPROVALRESULT']:checked").val();
        postData["APPROVALRESULT"] = approvalVal;

        $.SaveForm({
            url: "../../HiddenTroubleManage/HTApproval/SaveForm?keyValue=" + keyValue + "&submitUser=&isUpSubmit=1",
            param: postData,
            loading: "正在保存数据...",
            success: function () {
                $.currentIframe().$("#gridTable").trigger("reloadGrid");
            }
        })
    }


    //获取历史评估记录
    function GetHistoryApproval() {
        var keyCode = $("#HIDCODE").val();
        dialogOpen({
            id: 'HistoryForm',
            title: '隐患历史评估记录',
            url: '/HiddenTroubleManage/HTApproval/DetailList?keyCode=' + keyCode,
            width: ($(top.window).width() - 200) + "px",
            height: ($(top.window).height() - 200) + "px",
            btn: null
        });
    }

</script>

